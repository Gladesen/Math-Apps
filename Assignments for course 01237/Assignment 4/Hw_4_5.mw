<Startup-Code startupcode="###########################################################\012# M\303\266bius Hjemmeopgaves\303\246t 4 til 01237 Differentialgeometri \012#  og parametrisk design\012#  Opgave 5: Cosserat Sweep Flade II\012#     - Line Glade, 06/10/22\012###########################################################\012restart;\012with(combinat): with(ListTools):with(LinearAlgebra):\012with(RandomTools): with(plots): with(plottools): with(ArrayTools):\012\012macro(\012SP = DocumentTools:-SetProperty, \012GP = DocumentTools:-GetProperty,\012RG = RandomTools:-Generate\012):\012\012Actions := module()\012\009export Initialize, GetInput, SetProblem, Grade, Reset, InputCheck;\012\009local  SOL;\012\009global GradTotal, Grad, TEST, grad, report, interpretation, Rx, Ry, Rz, t, u;\012\012\009# Test variable: Set to false before export\012\009TEST := false;\012\012\009########################################\012\009# Initialization\012\009########################################\012\009Initialize:= proc()\012\009\009Grad[1] := 0.0; Grad[2] := 0.0;\012\009\009SP('Input',value,NULL);\012\009\009SP('Interpreter',value,NULL);\012\009\009SP('Reporter',value,NULL);\012\009\009SP('GradeTotal',value,NULL);\012\012\009\009SP(cat('RadioButton',2),'value', true); # Resetting radio buttons\012\012\009\009grad := 0;\012\009\009interpretation := &quot;&quot;;\012\009\009report := &quot;&quot;;\012\009\009\012\009\009## Test\012\009\009if evalb(TEST = true) then\012\009\009\009SP('Test1','visible',true);\012\009\009\009SP('Test2','visible',true);\012\009\009else\012\009\009\009SP('Test1','visible',false);\012\009\009\009SP('Test2','visible',false);\012\009\009end if;\012\009\009\012\009\009Actions:-SetProblem();\012\009\009\012\009end proc:\012\012\009########################################\012\009# Defining rotation matrices\012\009########################################\012\009Rx := proc()\012\009\009###########################################################\012\009\009description &quot;Rotation matrix angle  t  around x-axis. Output Matrix.&quot;;\012\009\009###########################################################\012\009\009Matrix([\012\009\009[1, 0,0],\012\009\009[0, cos(t), -sin(t)], \012\009\009[0, sin(t), cos(t)]\012\009\009]); \012\009#############################################################\012\009end proc:\012\012\009Ry:= proc()\012\009\009###########################################################\012\009\009description &quot;Rotation matrix angle  t  around y-axis. Output Matrix.&quot;;\012\009\009###########################################################\012\009\009Matrix([\012\009\009[cos(t), 0, -sin(t)], \012\009\009[0,1,0],\012\009\009[sin(t), 0,  cos(t)]\012\009\009]); \012\012\009#############################################################\012\009end proc:\012\009\012\009Rz:= proc()\012\009\009###########################################################\012\009\009description &quot;Rotation matrix angle  t  around z-axis. Output Matrix.&quot;;\012\009\009###########################################################\012\009\009Matrix([\012\009\009[cos(t), -sin(t), 0], \012\009\009[sin(t), cos(t), 0],\012\009\009[0,0,1]\012\009\009]); \012\012\009#############################################################\012\009end proc:\012\009\012\009########################################\012\009# Setting up the problem\012\009########################################\012\009SetProblem := proc()\012\009\009local a, b, c, d, e, r, p, w, R, rt, ru, Jacobi_r, A; #Sol1\012\009\009# The parametric equation is regular if Jacobi &gt; 0 inside the given interval.\012\009\009randomize();\012\012\009\009a := RG(integer(range=1..5));\012\009\009b := RG(integer(range=1..5));\012\009\009c := RG(integer(range=1..5));\012\009\009d := RG(integer(range=1..5));\012\009\009e := RG(integer(range=1..5));\012\009\009\009\012\009\009p := t-&gt; &lt;a*cos(t), b, c&gt;;\012\009\009w := u-&gt; &lt; d, 0, e*cos(u)&gt;;\012\009\009R := t-&gt; Rz();\012\009\009\012\009\009SP('Given1','value',[p(t)[1],p(t)[2],p(t)[3]]);\012\009\009SP('Given2','value',[w(u)[1],w(u)[2],w(u)[3]]);\012\012\009\009r := (t,u)-&gt; p(t) + R(t).w(u);\012\009\009rt := (t,u)-&gt; diff(r(t,u),t);\012\009\009ru := (t,u)-&gt; diff(r(t,u),u);\012\009\009\012\009\009Jacobi_r := simplify(Norm(CrossProduct(rt(t,u),ru(t,u)),2),'assume=real');\012\012\009\009# Checking if the parametric equation is regular\012\009\009#Sol1 := solve(Jacobi_r = 0);\012\012\009\009SOL[1] := 0;\012\009\009SP('Test1','value',&quot;Regular&quot;);\012\009\009\012\009\009A := evalf(simplify(int(Jacobi_r, t=-1..1, u = 1..2)));\012\009\009SP('Test2','value', evalf[8](A));\012\009\009SOL[2]:= [A];\012\009\009\012\012\009\009\012\009end proc:\012\012\009########################################\012\009# Input check\012\009########################################\012\009InputCheck := proc( raw::string, sol::list)\012\009\009####################################################\012\009\009local unpack, num, numsol, listnum, listnumsol, \012\009\009user, userflat, solflat, soldiff, solref, GO1, GO2, \012                names, differ, i, solflatfct, ran, q, w;\012\009\009global  grad, report, interpretation;\012\009\009####################################################\012\012\009\009GO1:= 1;\012\012\009\009try \012\009\009\009parse(raw); \012\009\009catch :\012\009\009\009GO1:= 0;\012\009\009end try;\012\009\009\012\009\009if evalb(GO1=1) then \012\009\009\009report:= &quot;&quot;; \012\009\009\009unpack:= [eval(parse(raw))];\012\009\009\009num:= nops(Flatten(unpack, 1));\012\009\009\009numsol:= nops(Flatten([sol], 1));\012\009\009\009listnum:= nops(Flatten(unpack));\012\009\009\009listnumsol:= nops(Flatten([sol]));\012\009\009\009if not(num=numsol and listnum=listnumsol and indets(evalf(unpack), name) = \012                            indets(evalf(sol), name)) then\012\009\009\009\009GO2:= 0; report:= &quot;Missing input or wrong input type&quot;; interpretation:= &quot;&quot;;\012\009\009\009else \012\009\009\009\009GO2:= 1; report:= &quot;&quot;;\012\009\009\009\009if (num=3 and listnum=9) or (num=2 and listnum=4) then \012\009\009\009\009\009interpretation:= Matrix(Flatten(unpack, 1));\012\009\009\009\009else \012\009\009\009\009\009interpretation:= Flatten(unpack, 1); \012\009\009\009\009end if;\012\009\009\012\009\009\009\009if (num=1 and listnum=1) then \012\009\009\009\009\009interpretation:= unpack[1];\012\009\009\009\009end if; \012\009\009\009end if;\012\009\009else \012\009\009\009report:= &quot;Syntax error&quot;; interpretation:= &quot;&quot;;\012\009\009end if;\012\009\009\012\009\009if evalb(GO1=1 and GO2=1) then \012\009\009\009user:= evalf(Flatten(unpack, 1));\012\009\009\009userflat:= evalf(Flatten(unpack));\012\009\009\009solflat:= evalf(Flatten(sol));\012\009\009\009\012\009\009\009names:= indets(evalf(unpack), name);\012\009\009\009\012\009\009\009if nops(names) &gt; 0 then \012\009\009\009\009\012\012\009\009\009\009differ:= unapply((solflat - userflat), seq(names[i], i=1...nops(names)));\012\009\009\009\009solflatfct:= unapply(solflat, seq(names[i], i=1...nops(names)));\012\009\009\009\009ran:= rand(-10...10);\012\009\009\012\009\009\009\009soldiff:= max(seq(evalf[3](Norm(Vector(differ(seq(ran(), \012                                               w=1...nops(names)))),2)), q=1...20));\012\012\009\009\009\009solref:= max(seq(evalf[3](Norm(Vector(solflatfct(seq(ran(), \012                                               w=1...nops(names)))),2)), q=1...20));\012\009\012\009\009\009else \012                    if solflat=[] and solflat=[] then \012                    \009soldiff:= 0.0; solref:= 1.0; \012                    else \012\009\009\009\009\009soldiff:= evalf[3](abs((solflat - userflat)[1]));\012\009\009\009\009\009solref:= evalf[3](abs(solflat[1]));\012                    end if\009\009\012\009\009\009end if;\012\009\009\012\009\009\009if soldiff &lt;= 0.001*solref then\012\009\009\009\009grad := 1.0;\012\009\009\009elif soldiff &lt;= 0.1*solref then\012\009\009\009\009grad:= 0.5;\012\009\009\009else\012\009\009\009\009grad:= 0.0;\012\009\009\009end if;\012\009\009else \012\009\009\009grad:= 0.0 \012\009\009end if;\012\009\009\012\009end proc:\012\012\009###################\012\009## GetInput \012\009###################\012\009GetInput := proc()\012\009\009local UserSol, i, userInput;\012\009\009Grad[1] := 0.0;\012\009\009Grad[2] := 0.0;\012\009\009\012\009\009\012\009\009# Radiobuttons\012\009\009for i from 1 to 3 do\012\009\009\009UserSol[i] := parse(GP(cat('RadioButton',i),'value'));\012\009\009end do;\012\012\009\009if (SOL[1] = 0) then\012\009\009\009if evalb(UserSol[1] = true) then\012\009\009\009\009Grad[1] := 1.0;\012\009\009\009end if;\012\009\009else\012\009\009\009if evalb(UserSol[3] = true) then\012\009\009\009\009Grad[1] := 1.0;\012\009\009\009end if;\012\009\009end if;\012\012\012\009\009# Area\012\009\009userInput := GP('Input',value);\012\009\009Actions:-InputCheck(userInput, SOL[2]);\012\009\009\012\009\009Grad[2] := grad;\012\009\009GradTotal := evalf[3]((Grad[1]+Grad[2])/2);\012\012\009\009SP('GradeTotal',value, GradTotal);\012\009\009\012\009\009if (report = &quot;&quot;) then\012\009\009\009SP('Interpreter','value',interpretation);\012\009\009else\012\009\009\009SP('Interpreter','value',NULL);\012\009\009end if;\012\009\009\012\009\009SP('Reporter','value',report);\012\012\012\009\009\012\009end proc:\012\012\009#######################################\012\009# RESET\012\009#######################################\012\009Reset:= proc()\012\009\009local i;\012\009\009Grad[1] := 0.0; Grad[2] := 0.0;\012\009\009\012\009\009SP(cat('RadioButton',2),'value', true); # Resetting radio buttons\012\009\009\012\012\009\009\012\009\009SP('Input',value,NULL);\012\009\009SP('Interpreter',value,NULL);\012\009\009SP('Reporter',value,NULL);\012\009\009SP('GradeTotal',value,NULL);\012\009\009\012\009\009Actions:-SetProblem();\012\009end proc:\012\012\009########################################\012\009# GRADE OUT\012\009########################################\012\009Grade := proc();\012\012     \009return GradTotal;\012\012\009end proc:\012\009\012########################################\009\009\012end module : # Actions module end\012########################################\012\012Actions:-Initialize();"/>
