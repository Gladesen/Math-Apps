<Startup-Code startupcode="###########################################################\012# M\303\266bius Hjemmeopgaves\303\246t 4 til 01237 Differentialgeometri \012#  og parametrisk design\012#  Opgave 2: Rotationsmatrix\012#     - Line Glade, 28/09/22, SM 07/10/22\012# Version 2:\012#   - Fixed issue where the rotation matrix was not a rotation matrix, since the determinant wasn't 1\012# Version 3: \012#   - Uses MatrixExponential for unique rotation matrix solution\012###########################################################\012restart;\012with(combinat): with(ListTools):with(LinearAlgebra):\012with(RandomTools): with(plots): with(plottools): with(ArrayTools):\012\012macro(\012SP = DocumentTools:-SetProperty, \012GP = DocumentTools:-GetProperty,\012RG = RandomTools:-Generate\012):\012\012##########################################################\012\012Actions := module()\012\009export Initialize, GetInput, SetProblem, Grade, Reset, InputCheck;\012\009local  solA, W, Omega;\012\009global Grad, TEST, grad, report, interpretation, t;\012\012\009# Test variable: Set to false before export\012\009TEST := false;\012\012\009########################################\012\009# Initialization\012\009########################################\012\009Initialize:= proc()\012\009\009SP('Input',value,NULL);\012\009\009SP('GradeTotal',value,NULL);\012\009\009SP('Reporter',value,NULL);\012\009\009SP('Interpreter',value,NULL);\012\009\009Grad := 0.0;\012\012\009\009Actions:-SetProblem();\012\012\009\009## Test\012\009\009if evalb(TEST = true) then\012\009\009\009SP('Test','visible',true);\012\009\009else\012\009\009\009SP('Test','visible',false);\012\009\009end if;\012\009\009\012\009end proc:\012\012   \009####################################################\012\009InputCheck:= proc(raw::string, sol::list)\012\009####################################################\012\009\009####################################################\012\009\009local unpack, num, numsol, listnum, listnumsol, \012\009\009user, userflat, solflat, soldiff, solref, GO1, GO2, \012                names, differ, i, solflatfct, ran, q, w;\012\009\009global  grad, report, interpretation;\012\009\009####################################################\012\012\009\009GO1:= 1;\012\012\009\009try \012\009\009\009parse(raw); \012\009\009catch :\012\009\009\009GO1:= 0;\012\009\009end try;\012\009\009\012\009\009if evalb(GO1=1) then \012\009\009\009report:= &quot;&quot;; \012\009\009\009unpack:= [eval(parse(raw))];\012\009\009\009num:= nops(Flatten(unpack, 1));\012\009\009\009numsol:= nops(Flatten([sol], 1));\012\009\009\009listnum:= nops(Flatten(unpack));\012\009\009\009listnumsol:= nops(Flatten([sol]));\012\009\009\009if not(num=numsol and listnum=listnumsol and indets(evalf(unpack), name) = indets(evalf(sol), name)) then\012\009\009\009\009GO2:= 0; report:= &quot;Forkert type eller manglende input&quot;; interpretation:= &quot;&quot;;\012\009\009\009else \012\009\009\009\009GO2:= 1; report:= &quot;&quot;;\012\009\009\009\009if (num=3 and listnum=9) or (num=2 and listnum=4) then \012\009\009\009\009\009interpretation:= Matrix(Flatten(unpack, 1));\012\009\009\009\009else \012\009\009\009\009\009interpretation:= Flatten(unpack, 1); \012\009\009\009\009end if;\012\009\009\012\009\009\009\009if (num=1 and listnum=1) then \012\009\009\009\009\009interpretation:= unpack[1];\012\009\009\009\009end if; \012\009\009\009end if;\012\009\009else \012\009\009\009report:= &quot;Syntaksfejl&quot;; interpretation:= &quot;&quot;;\012\009\009end if;\012\009\009\012\009\009if evalb(GO1=1 and GO2=1) then \012\009\009\009user:= evalf(Flatten(unpack, 1));\012\009\009\009userflat:= evalf(Flatten(unpack));\012\009\009\009solflat:= evalf(Flatten(sol));\012\009\009\009\012\009\009\009names:= indets(evalf(unpack), name);\012\009\009\009\012\009\009\009if nops(names) &gt; 0 then \012\009\009\009\009\012\009\009\009\009differ:= unapply((solflat - userflat), seq(names[i], i=1...nops(names)));\012\009\009\009\009solflatfct:= unapply(solflat, seq(names[i], i=1...nops(names)));\012\009\009\009\009ran:= rand(-10...10);\012\009\009\009\009soldiff:= max(seq(evalf[3](Norm(Vector(differ(seq(ran(), \012                                               w=1...nops(names)))),2)), q=1...20));\012\009\009\009\009solref:= max(seq(evalf[3](Norm(Vector(solflatfct(seq(ran(), \012                                               w=1...nops(names)))),2)), q=1...20));\012\009\009\009else \012                    if solflat=[] and solflat=[] then \012                    \009soldiff:= 0.0; solref:= 1.0; \012                    else \012\009\009\009\009\009soldiff:= evalf[3](abs((solflat - userflat)[1]));\012\009\009\009\009\009solref:= evalf[3](abs(solflat[1]));\012                    end if\009\009\012\009\009\009end if;\012\009\009\012\009\009\009if soldiff &lt;= 0.001*solref then\012\009\009\009\009grad := 1.0;\012\009\009\009elif soldiff &lt;= 0.1*solref then\012\009\009\009\009grad:= 0.5;\012\009\009\009else\012\009\009\009\009grad:= 0.0;\012\009\009\009end if;\012\009\009else \012\009\009\009grad:= 0.0 \012\009\009end if;\012\012\009####################################################\012\009end proc:\012\009####################################################\012\009\012\009########################################\012\009# Setting up the problem\012\009########################################\012\009SetProblem := proc()\012\009\009local abc, a, alpha, beta, gam, Omega, ran1, ran ;\012\012\009\009randomize();\012                ran1:= rand(2...15);\012                ran:= rand(1...3);\012                a:= ran1();\012                abc:= [[a, 0, 0], [0,a,0], [0,0,a]][ran()]; \012                alpha:= abc[1]; beta:= abc[2]; gam:= abc[3]; \012                Omega:= Matrix(3,3, [[0, -gam, beta], [gam, 0, -alpha], [-beta, alpha, 0]]);\012\012\009\009W:= MatrixExponential(Omega*t);\012\009\009solA:= convert(W, listlist);\012\012\009\009SP('Given1',value,Omega);\012\009\009\012\009\009SP('Test',value,solA);\012\012\009\009\012\009end proc:\012\012\009###################\012\009## GetInput \012\009###################\012\009GetInput := proc()\012\009\009local UserInput;\012\012\009\009UserInput := GP('Input',value);\012\009\009InputCheck( UserInput, solA);\012\009\009Grad := grad;\012\012\009\009SP('GradeTotal',value, Grad);\012\009\009\012\009\009if (report = &quot;&quot;) then\012\009\009\009SP('Interpreter','value',interpretation);\012\009\009else\012\009\009\009SP('Interpreter','value',NULL);\012\009\009end if;\012\009\009\012\009\009SP('Reporter','value',report);\012\009\009\012\009\009\012\009end proc:\012\012\009#######################################\012\009# RESET\012\009#######################################\012\009Reset:= proc()\012\009\009SP('Input',value,NULL);\012\009\009SP('GradeTotal',value,NULL);\012\009\009SP('Reporter',value,NULL);\012\009\009SP('Interpreter',value,NULL);\012\009\009Actions:-SetProblem();\012\009end proc:\012\012\009########################################\012\009# GRADE OUT\012\009########################################\012\009Grade := proc();\012\012     \009return Grad;\012\012\009end proc:\012\009\012########################################\009\009\012end module : # Actions module end\012########################################\012\012Actions:-Initialize();\012\012########################################"/>
