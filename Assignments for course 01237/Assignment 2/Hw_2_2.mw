<Startup-Code startupcode="###########################################################\012# M\303\266bius Hjemmeopgaves\303\246t 2 til 01237 Differentialgeometri \012#  og parametrisk design\012#\012# Opgave 2\012#  \009Final version (12.09)\012#  \009- Changed test-variable\012###########################################################\012reset:\012with(combinat): with(ListTools):with(LinearAlgebra): with(RandomTools):\012\012macro(\012SP = DocumentTools:-SetProperty, \012GP = DocumentTools:-GetProperty,\012RG = RandomTools:-Generate\012):\012\012#Digits := 3:\012Actions := module()\012\009export Initialize, GetInput, SetProblem, Grade, Reset;\012\009global SOL, Grad, w, GRADE, TEST;\012\009#####################\012\009TEST := false;\012\009#####################\012\012\009SetProblem[2] := proc()\012\009\009local ans, r, T1, T2, a, b, i, j, F;\012\009\009w := 2;\012\009\009randomize();\012\012\009\009T1 := Generate('Matrix'(integer(range = -3 .. 5), 3, 3));\012\009\009T2 := Generate('Matrix'(integer(range = -3 .. 5), 3, 3));\012\012\009\009r := [T1, T2];\012\009\009\012\009\009for i from 1 to 2 do\012\009\009\009F := &quot;(&quot;;\012\009\009\009for j from 1 to 3 do\012\009\009\009\009F := cat(F,&quot;(&quot;, r[i][j][1], &quot;,&quot;, r[i][j][2], &quot;,&quot;, r[i][j][3], &quot;)&quot;);\012\009\009\009\009if not evalb(j = 3) then\012\009\009\009\009\009F := cat(F, &quot;,&quot;);\012\009\009\009\009end if;\012\009\009\009end do;\012\009\009\009F := cat(F, &quot;)&quot;);\012\009\009\009SP(cat('TextArea',i+1+w*10),'value',F);\012\012\009\009\009a := &lt;r[i][2][1],r[i][2][2],r[i][2][3]&gt;;\012\009\009\009b := &lt;r[i][3][1],r[i][3][2],r[i][3][3]&gt;;\012\009\009\009\012\009\009\009SOL[i] := (1/2)*Norm(CrossProduct(a,b),2);\012\009\009\009\012\009\009end do;\012\012\009\009SP(cat('test',w),'value', [SOL[1], SOL[2]]);\012\009\009\012\009end proc:\012\012\009########################################\012\009# Initialization\012\009########################################\012\009Initialize:= proc()\012\009\009local i;\012\009\009w := 2;\012\009\009Grad[1] := 0; Grad[2] := 0;\012\012\009\009for i from 2 to 8 do\012\009\009\009SP(cat('TextArea',i+w*10),'value',NULL);\012\009\009end do;\012\009\009SP('test2','value',NULL);\012\009\009\012\009\009SP('MathContainer10','value',NULL);\012\009\009SP('MathContainer11','value',NULL);\012\012\009\009SP('InputCheck1', 'value', &quot;Wrong input type&quot;);\012\009\009SP('InputCheck2', 'value', &quot;Wrong input type&quot;);\012\009\009SP('InputCheck1', 'visible', false);\012\009\009SP('InputCheck2', 'visible', false);\012\009\009\012\009\009SetProblem[w]();\012\012\009\009###################\012\009\009## Test\012\009\009###################\012\009\009if TEST = true then\012\009\009\009SP(cat('test',w),'visible',true);\009\012\009\009else\012\009\009\009SP(cat('test',w),'visible',false);\012\009\009end if;\012\009\009\012\009end proc:\012\009\012\009########################################\012\009# Input check\012\009########################################\012\009GetInput[2] := proc( n )\012\009\009local UserSol, uRaw, soldiff, solcrit, grad, catch_input;\012\009\009w := 2;\012\012\009\009uRaw := GP(cat('TextArea',w*10+3+n),'value');\012\009\009\012\009\009if (uRaw = &quot;&quot;) then\012\009\009\009grad := 0.0;\012\009\009else\012\009\009\009try\012\009\009\009\009UserSol := parse(uRaw);\012\009\009\009catch :\012\009\009\009\009catch_input := 1;\012\009\009\009end try;\012\012\009\009\009if evalb(catch_input = 1) then\012\009\009\009\009grad := 0.0;\012\009\009\009\009SP(cat('MathContainer',9+n),'value',Error);\012\009\009\009\009SP(cat('InputCheck',n),'visible',true);\012\009\009\009else\012\009\009\009\009SP(cat('InputCheck',n),'visible',false);\012\009\009\009\009SP(cat('MathContainer',9+n),'value',UserSol);\012\012\009\009\009\009if evalb(whattype(evalf(UserSol)) = float) then\012\009\009\009\009\009soldiff := abs(evalf[3](UserSol) - evalf[3](SOL[n])); # evaluating the first three digits\012\009\009\009\009\009solcrit := evalf(0.1*SOL[n]);\012\012\009\009\009\009\009if evalb(soldiff = 0) then\012\009\009\009\009\009\009grad := 1.0;\012\009\009\009\009\009elif evalb(soldiff &lt;= solcrit) then\012\009\009\009\009\009\009grad := 0.5;\012\009\009\009\009\009else\012\009\009\009\009\009\009grad := 0.0;\012\009\009\009\009\009end if;\012\009\009\009\009else\012\009\009\009\009\009grad := 0.0;\012\009\009\009\009\009SP(cat('InputCheck',n),'visible', true);\012\009\009\009\009end if;\012\009\009\009end if;\012\009\009end if;\012\012\009\009Grad[n] := grad;\012\009\009\012\009\009SP(cat('TextArea',5+10*w+n),'value',Grad[n]);\012\012\009end proc:\012\012\009########################################\012\009# Grade\012\009########################################\012\009Grade := proc()\012\009\009w := 2;\012\009\009if not (Grad[1] = 1.0) then\012\009\009\009Actions:-GetInput(1);\012\009\009end if;\012\009\009if not (Grad[2] = 1.0) then\012\009\009\009Actions:-GetInput(2);\012\009\009end if;\012\009\009\012\009\009\012\009\009GRADE := evalf( 0.5*(Grad[1]+Grad[2]));\012\009\009SP('TextArea28','value', GRADE);\012\009\009return GRADE;\012\009end proc:\012\012\009#######################################\012\009# Reset\012\009#######################################\012\009Reset[2]:= proc()\012\009\009local w, i;\012\009\009w := 2;\012\009\009Grad[1] := 0; Grad[2] := 0;\012\012\009\009for i from 2 to 8 do\012\009\009\009SP(cat('TextArea',i+w*10),'value',NULL);\012\009\009end do;\012\009\009SP('test2','value',NULL);\012\009\009\012\009\009SP('MathContainer10','value',NULL);\012\009\009SP('MathContainer11','value',NULL);\012\009\009\012\009\009SP('InputCheck1', 'visible', false);\012\009\009SP('InputCheck2', 'visible', false);\012\009\009SetProblem[w]();\012\009end proc:\012\012\009\012########################################\009\009\012end module: # Actions module end\012########################################\012\012Actions:-Initialize():\012\012########################################"/>
