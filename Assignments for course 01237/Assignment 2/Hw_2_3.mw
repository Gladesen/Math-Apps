<Startup-Code startupcode="###########################################################\012# M\303\266bius Hjemmeopgaves\303\246t 2 til 01237 Differentialgeometri \012#  og parametrisk design\012#\012# Opgave 3\012#  \009Final version (12.09)\012#  \009- Changed test-variable\012###########################################################\012reset;\012with(combinat): with(ListTools):with(LinearAlgebra): with(RandomTools):\012\012macro(\012SP = DocumentTools:-SetProperty, \012GP = DocumentTools:-GetProperty,\012RG = RandomTools:-Generate\012):\012\012#Digits := 3:\012Actions := module()\012\009export Initialize, GetInput, SetProblem, Grade, Reset;\012\009global SOL, Grad, w, TEST, GRADE;\012\009#####################\012\009TEST := false;\012\009#####################\012\012\009SetProblem := proc()\012\009\009local ans, r, F, i, j, T1, T2, T3, T4, T5, a, b, c;\012\009\009w := 3;\012\009\009randomize();\012\012\009\009T1 := Generate('Matrix'(integer(range = -3 .. 5), 4, 3));\012\009\009T2 := Generate('Matrix'(integer(range = -3 .. 5), 4, 3));\012\009\009T3 := Generate('Matrix'(integer(range = -3 .. 5), 4, 3));\012\009\009T4 := Generate('Matrix'(integer(range = -3 .. 5), 4, 3));\012\009\009T5 := Generate('Matrix'(integer(range = -3 .. 5), 4, 3));\012\012\009\009r := [T1, T2, T3, T4, T5];\012\012\009\009for i from 1 to 5 do\012\009\009\009F := &quot;(&quot;;\012\009\009\009for j from 1 to 4 do\012\009\009\009\009F := cat(F,&quot;(&quot;, r[i][j][1], &quot;,&quot;, r[i][j][2], &quot;,&quot;, r[i][j][3], &quot;)&quot;);\012\009\009\009\009if not evalb(j = 4) then\012\009\009\009\009\009F := cat(F, &quot;,&quot;);\012\009\009\009\009end if;\012\009\009\009end do;\012\009\009\009F := cat(F, &quot;)&quot;);\012\009\009\009SP(cat('TextArea',i+10+w*100),'value',F);\012\012\009\009\009# Determining the answers\012\009\009\009a := &lt;r[i][2][1],r[i][2][2],r[i][2][3]&gt;;\012\009\009\009b := &lt;r[i][3][1],r[i][3][2],r[i][3][3]&gt;;\012\009\009\009c := &lt;r[i][4][1],r[i][4][2],r[i][4][3]&gt;;\012\009\009\009\012\009\009\009ans[i] :=  (1/6)*abs(Determinant(&lt;a | b | c&gt;));\012\009\009\009\012\009\009end do;\012\009\009SOL :=  [ans[1], ans[2], ans[3], ans[4], ans[5]];\012\009\009SP(cat('test',w),'value',SOL);\012\009\009\012\009end proc:\012\012\009########################################\012\009# Initialization\012\009########################################\012\009Initialize:= proc()\012\009\009local i;\012\009\009w := 3;\012\009\009GRADE := 0;\012\009\009for i from 1 to 5 do\012\009\009\009SP(cat('Answer',i),'value',NULL);\012\009\009\009SP(cat('Input',i),'value',NULL);\012\009\009\009SP(cat('Grade',i),'value',NULL);\012\009\009\009SP(cat('InputCheck',i),'value', &quot;Forkert type input.&quot;);\012\009\009\009SP(cat('InputCheck',i),'visible', false);\012\009\009\009Grad[i] := 0;\012\009\009end do;\009\009\012\009\009SP('Grade','value', NULL);\012\009\009\012\009\009SetProblem();\012\012\009\009###################\012\009\009## Test\012\009\009###################\012\009\009if TEST = true then\012\009\009\009SP(cat('test',w),'visible',true);\009\012\009\009else\012\009\009\009SP(cat('test',w),'visible',false);\012\009\009end if;\012\009\009\012\009end proc:\012\009\012\009########################################\012\009# Input check\012\009########################################\012\009GetInput := proc( n )\012\009\009local UserSol, uRaw, catch_input, grad, soldiff, solcrit;\012\009\009catch_input := 0; grad := 0.0;\012\009\009\012\009\009uRaw := GP(cat('Answer',n),'value');\012\012\009\009if evalb(uRaw = &quot;&quot;) then\012\009\009\009grad := 0.0;\012\009\009else\012\009\009\009try \012\009\009\009\009UserSol := parse(uRaw);\012\009\009\009catch :\012\009\009\009\009catch_input := 1;\012\009\009\009end try;\012\012\009\009\009if evalb(catch_input = 1) then\012\009\009\009\009grad := 0.0;\012\009\009\009\009SP(cat('Input',n),'value',Error);\012\009\009\009\009SP(cat('InputCheck',n),'visible', true);\012\009\009\009else\012\009\009\009\009SP(cat('Input',n),'value', UserSol);\012\009\009\009\009SP(cat('InputCheck',n),'visible', false);\012\009\009\009\009if evalb(whattype(evalf(UserSol)) = float) then\012\009\009\009\009\009soldiff := abs(evalf[3](UserSol - SOL[n]));\012\009\009\009\009\009solcrit := eval(0.1*SOL[n]);\012\009\009\009\009\009\012\009\009\009\009\009if evalb(soldiff = 0) then\012\009\009\009\009\009\009grad := 1.0;\012\009\009\009\009\009elif evalb( soldiff &lt; solcrit) then\012\009\009\009\009\009\009grad := 0.5;\012\009\009\009\009\009else\012\009\009\009\009\009\009grad := 0.0;\012\009\009\009\009\009end if;\012\012\009\009\009\009end if;\012\009\009\009end if;\012\009\009end if;\012\012\009\009Grad[n] := grad;\012\009\009SP(cat('Grade',n),'value',grad);\012\012\012\009\009SP('Grade','value', evalf(0.2*(Grad[1]+Grad[2]+Grad[3]+Grad[4]+Grad[5])));\012\009\009\012\009\009#Actions:-Grade();\012\009\009\012\009end proc:\012\012\009########################################\012\009# Grade\012\009########################################\012\009Grade := proc()\012\009\009local i;\012\009\009for i from 1 to 5 do\012\009\009\009if not (Grad[i] = 1.0) then\012\009\009\009\009Actions:-GetInput(i);\012\009\009\009end if;\012\009\009end do;\012\012\009\009GRADE := evalf( 0.2 * (Grad[1]+Grad[2]+Grad[3]+Grad[4]+Grad[5]));\012\009\009SP('Grade','value',GRADE);\012\009\009return GRADE;\012\009end proc:\012\012\009#######################################\012\009# Reset\012\009#######################################\012\012\009Reset:= proc()\012\009\009local i;\012\009\009GRADE := 0;\012\009\009SP('test3','value', NULL);\012\009\009for i from 1 to 5 do\012\009\009\009SP(cat('TextArea',i+310),'value',NULL);\012\009\009\009SP(cat('Answer',i),'value',NULL);\012\009\009\009SP(cat('Input',i),'value',NULL);\012\009\009\009SP(cat('Grade',i),'value',NULL);\012\009\009\009SP(cat('InputCheck',i),'visible', false);\012\009\009\009Grad[i] := 0;\012\009\009end do;\009\009\012\009\009\012\009\009SP('Grade','value', NULL); \012\009\009SetProblem();\012\009\009return;\012\009end proc:\012\012\009\012########################################\009\009\012end module: # Actions module end\012########################################\012\012Actions:-Initialize():\012\012########################################"/>
