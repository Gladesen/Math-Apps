<Startup-Code startupcode="######################################################################\012# Assignment 2 for 01237 Differential geometry and parametric design\012#\012# Question 9 (by Steen Markvorsen)\012#  \009Final version (12.09)\012#  \009- Changed test-variable\012#    Fix (19.09)\012#    - Moved InputCheck, Rx and Rz into the Actions module\012#    - Made variables from InputCheck procedure global\012#    - Added return statement in Grade procedure\012######################################################################\012\012restart;\012with(combinat): with(ListTools): with(LinearAlgebra): with(RandomTools):\012with(plots): with(plottools):\012\012macro(\012SP = DocumentTools:-SetProperty, \012GP = DocumentTools:-GetProperty,\012RG = RandomTools:-Generate\012):\012\012\012############################################################\012# ACTIONS\012############################################################\012Actions := module()\012\009export Initialize, GetInput, SetProblem, Grade, Reset, InputCheck, Rx, Rz;\012\009local solA, solAlist, solBlist;\012\009global   GradA, GradB, GradTotal, TEST, grad, report, interpretation, \012\009interpretationA, interpretationB, reportA, reportB;\012\012\009#####################\012\009TEST := false;\012\009#####################\012\012\009# Helper functions:\012\009# InputCheck\012\009# Rx and Rz (rotation matrices)\012\009\012\009####################################################\012\009InputCheck:= proc(raw::string, sol::list)\012\009####################################################\012\009\009description &quot;Input usersolution raw as string from input TextArea. Input solution sol as a list: number, vector-list, matrix-list in 2D or 3D. NB: Even if the solution is 7.3 then sol:=[7.3] must be used. Output: The grad grad, a report (possibly empty) and the interpretation of the input if no syntaks or type are encountered.&quot;;\012\009\009####################################################\012\009\009local unpack, num, numsol, listnum, listnumsol, \012\009\009userflat, solflat, soldiff, solcrit, GO1, GO2, i;\012\009\009global  grad, report, interpretation;\012\009\009####################################################\012\009\009grad := 0.0;\012\009\009GO1:= 1;\012\009\009\012\009\009try \012\009\009\009parse(raw); \012\009\009catch :\012\009\009\009GO1 := 0;\012\009\009end try;\012\009\009\012\009\009if evalb(GO1 = 1) then \012\009\009\009report:= &quot;&quot;; \012\009\009\009unpack:= [parse(raw)];\012\009\009\009num:= nops(Flatten(unpack, 1));\012\009\009\009numsol:= nops(Flatten([sol], 1));\012\009\009\009listnum:= nops(Flatten(unpack));\012\009\009\009listnumsol:= nops(Flatten([sol]));\012\009\009\009\012\009\009\009if not(num=numsol and listnum=listnumsol and whattype(evalf(add(Flatten(unpack)[i], i=1...listnum)))=float) then\012\009\009\009\009GO2:= 0; report:= &quot;Missing input or wrong input type&quot;; interpretation:= &quot;&quot;;\012\009\009\009else \012\009\009\009\009GO2:= 1; report:= &quot;&quot;;\012\009\009\009\009if (num=3 and listnum=9) or (num=2 and listnum=4) then\012\009\009\009\009\009interpretation:= Matrix(Flatten(unpack, 1));\012\009\009\009\009else \012\009\009\009\009\009interpretation:= Flatten(unpack, 1); \012\009\009\009\009end if;\012\009\012\009\009\009\009if (num=1 and listnum=1) then \012\009\009\009\009\009interpretation:= unpack[1];\012\009\009\009\009end if; \012\009\009\009end if;\012\009\009else \012\009\009\009report:= &quot;Syntax error&quot;; interpretation:= &quot;&quot;;\012\009\009end if;\012\009\009\012\009\009if evalb(GO1=1 and GO2=1) then \012\009\009\009#user:= evalf(Flatten(unpack, 1));\012\009\009\009userflat:= evalf(Flatten(unpack));\012\009\009\009solflat:= evalf(Flatten(sol));\012\009\009\009soldiff := abs(evalf[3](Norm(Vector(userflat-solflat), 2)));\012\009\009\009solcrit := evalf[3](0.1*Norm(Vector(solflat), 2));\012\009\009\009if soldiff &lt;0.01 then\012\009\009\009\009grad := 1.0;\012\009\009\009elif soldiff &lt;= solcrit then\012\009\009\009\009grad:= 0.5;\012\009\009\009else\012\009\009\009\009grad:= 0.0;\012\009\009\009end if;\012\009\009else \012\009\009\009grad:= 0.0; \012\009\009end if;\012\009\012\009####################################################\012\009end proc:\012\009####################################################\012\009\012\009\012\009####################################################\012\009Rx:= proc(t)\012\009\009description &quot;Rotation matrix angle  t  around x-axis. Output Matrix.&quot;;\012\009\009option `Rev. 30.10.09 (S.Markvorsen@mat.dtu.dk)`;\012\009\009\012\009\009Matrix([\012\009\009[1, 0,0],\012\009\009[0, cos(t), -sin(t)], \012\009\009[0, sin(t), cos(t)]\012\009\009]); \012\009\012\009end proc :\012\009\012\009#############################################################\012\009\012\009Rz:= proc(t)\012\009\009description &quot;Rotation matrix angle  t  around z-axis. Output Matrix.&quot;;\012\009\009option `Rev. 30.10.09 (S.Markvorsen@mat.dtu.dk)`;\012\009\009\012\009\009Matrix([\012\009\009[cos(t), -sin(t), 0], \012\009\009[sin(t), cos(t), 0],\012\009\009[0,0,1]\012\009\009]); \012\009\012\009\012\009\012\009end proc:\012\009#############################################################\012\012\012\009############################################################\012\009# SETPROBLEM\012\009############################################################\009\012\009SetProblem := proc()\012\009\009local ran, sigma1, sigma2, sigma3, sig ;\012\012\009\009# GENERATING RANDOM 3 singular values squared\012\009\009randomize();     \012\009\009ran:= rand(1...14);\012\009\009\012\009\009sigma1:= ran(); \012\009\009sigma2:= ran(); \012\009\009sigma3:= ran();\012\012\009\009sig:= sort([sigma1, sigma2, sigma3], `&gt;`);\012\012\009\009solBlist:= [1/sig[3], 1/sig[2], 1/sig[1]];\012\009\009solA:= Rx(Pi/4).Matrix(3,3,[[sig[1],0,0], [0, sig[2], 0], [0, 0, sig[3]]]).Rz(-Pi/4);\012\009\009solAlist:= convert(solA, listlist);\012\012\009\009# PRESENTING: \012\009\009SP('Given1','value', sigma1^2);\012\009\009SP('Given2','value', sigma2^2);\012          SP('Given3','value', sigma3^2); \012\012\009\009# PRESENTING the solutions:\012\009\009SP('TestA','value', solAlist);\012\009\009SP('TestB','value', solBlist);\012\012\009end proc:\012\012\012\009########################################\012\009# INITIALIZATION\012\009########################################\012\009Initialize:= proc();\012\012\009\009GradA := 0.0; GradB:= 0.0;\012\009\009GradTotal := 0.0;\012\009\009\012\009\009SP('UserAreaA','value', NULL);\012\009\009SP('UserAreaB','value', NULL);                \012\009\009SP('GraderA','value', NULL);\012          SP('GraderB','value', NULL);\012\009\009SP('GraderTotal','value', NULL);\012\009\009SP('InterpreterA','value',NULL);\012\009     SP('InterpreterB','value',NULL);\012          SP('ReportA','visible',true);\012          SP('ReportB','visible',true);\012\009\009SP('ReportA','value',NULL);\012\009     SP('ReportB','value',NULL);\012\012\009\009###################\012\009\009## Test\012\009\009###################\012\009\009if TEST = false then\012\009\009\009SP('TestA','visible',false);\009\012\009\009\009SP('TestB','visible',false);\012\009\009else\012\009\009\009SP('TestA','visible',true);\009\012\009\009\009SP('TestB','visible',true);\012\009\009end if;\012\009\009\012\009\009SetProblem();\012\009\009\012     end proc:\012      \009\012      \009\012\009########################################\012\009# GET INPUT AND CHECK AND GRADE\012\009########################################\012\009GetInput := proc()\012\009\009local UserAreaA, UserAreaB;\012      \009global grad, report, interpretation, \012\009\009GradA, reportA, interpretationA, GradB, reportB, interpretationB, GradTotal;\012\009\009\012          UserAreaA:=  GP('UserAreaA','value');\009\009\012\009\009InputCheck(UserAreaA, solAlist);\012\009\009GradA:= grad; reportA:= report; interpretationA:= interpretation;\012\012\012         \009UserAreaB:=  GP('UserAreaB','value');\012\009\009InputCheck(UserAreaB, solBlist);\012\009\009GradB:= grad; reportB:= report; interpretationB:= interpretation;\012\012          GradTotal:= 0.5*(GradA + GradB);\012\012\009\009SP('GraderA','value', GradA);\012\009\009SP('GraderB','value', GradB);\012\009\009SP('GraderTotal','value', GradTotal);\012\012\012\009\009if (reportA=&quot;&quot;) then \012\009\009\009SP('InterpreterA','value', interpretationA); \012\009\009else  \012\009\009\009SP('InterpreterA','value', NULL); \012\009\009end if;\012\012\009\009if (reportB=&quot;&quot;) then \012\009\009\009SP('InterpreterB','value', interpretationB); \012\009\009else \012\009\009\009SP('InterpreterB','value', NULL);\009\012\009\009end if;\012\009    \012          SP('ReportA','value', reportA);\012\009     SP('ReportB','value', reportB);\012  \012\009end proc:\012\012\009########################################\012\009# GRADE OUT\012\009########################################\012\012\009Grade := proc();\012\012\009#\009if not(GradTotal  = 1.0) then\012\009#\009\009Actions:-GetInput();\012\009#\009end;\012\009\009return GradTotal;\012\012\009end proc:\012\012\012\009#######################################\012\009# RESET\012\009#######################################\012\009Reset:= proc()\012\012\009     SP('UserAreaA','value', NULL);\012\009\009SP('UserAreaB','value', NULL);  \012\009\009SP('GraderA','value', NULL);\012          SP('GraderB','value', NULL);              \012\009\009SP('GraderTotal','value', NULL);\012\009\009SP('InterpreterA','value',NULL);\012\009     SP('InterpreterB','value',NULL);\012          SP('ReportA','value',NULL);\012\009     SP('ReportB','value',NULL);\012\012          SetProblem();\012\009     \012\009end proc:\012\012########################################\009\009\012end module : # Actions module end\012########################################\012\012Actions:-Initialize():\012########################################"/>
