<Startup-Code startupcode="###########################################################\012# M\303\266bius Hjemmeopgaves\303\246t 2 til 01237 Differentialgeometri \012#  og parametrisk design\012#\012# Opgave 5\012#  \009Version 3 (07.09)\012#  \009- Added randomization\012# \009- Used grading homogenization\012#    - Removed variables for penalties for multiple tries and resets\012###########################################################\012restart;\012with(combinat): with(ListTools):with(LinearAlgebra): with(RandomTools):\012\012macro(\012SP = DocumentTools:-SetProperty, \012GP = DocumentTools:-GetProperty,\012RG = RandomTools:-Generate\012):\012\012#Digits := 3:\012Actions := module()\012\009export Initialize, GetInput, SetProblem, Grade, Reset;\012\009global SOL, Grad, GRADE, TEST;\012\009#####################\012\009TEST := false;\012\009#####################\012\012\009SetProblem := proc()\012\009\009local ans, i, j, r, F, T1, T2, T3, a, b, c, det, count;\012\009\009randomize();\012\012\009\009count[1] := 0; count[2] := 0;\012\012\009\009T1 := Generate('Matrix'(integer(range = -3 .. 5), 4, 3));\012\009\009T2 := Generate('Matrix'(integer(range = -3 .. 5), 4, 3));\012\009\009T3 := Generate('Matrix'(integer(range = -3 .. 5), 4, 3));\012\012\009\009r := [T1, T2, T3];\012\009\009for i from 1 to 3 do\012\009\009\009F := &quot;(&quot;;\012\009\009\009for j from 1 to 4 do\012\009\009\009\009F := cat(F,&quot;(&quot;, r[i][j][1], &quot;,&quot;, r[i][j][2], &quot;,&quot;, r[i][j][3], &quot;)&quot;);\012\009\009\009\009if not evalb(j = 4) then\012\009\009\009\009\009F := cat(F, &quot;,&quot;);\012\009\009\009\009end if;\012\009\009\009end do;\012\009\009\009F := cat(F, &quot;)&quot;);\012\009\009\009SP(cat('TextArea',i),'value',F);\012\009\009\012\009\009\009# Determining the answers\012\009\009\009a := &lt;r[i][2][1],r[i][2][2],r[i][2][3]&gt;;\012\009\009\009b := &lt;r[i][3][1],r[i][3][2],r[i][3][3]&gt;;\012\009\009\009c := &lt;r[i][4][1],r[i][4][2],r[i][4][3]&gt;;\012\012\009\009\009det := Determinant(&lt;a|b|c&gt;);\012\009\009\009# The tetrahedrons are positively oriented if the determinant is positive\012\009\009\009if (det &gt; 0) then\012\009\009\009\009ans[i] := true;\012\009\009\009\009count[1] := count[1] + 1;\012\009\009\009else\012\009\009\009\009ans[i] := false;\012\009\009\009\009count[2] := count[2] + 1;\012\009\009\009end if;\012\012\009\009\009SOL :=  [ans[1], ans[2], ans[3]];\012\009\009\009SP('test','value',SOL);\012\012\009\009#\009if (count[1] = 3) or (count[2] = 3) then\012\009\009#\009\009Actions:-SetProblem():\012\009\009#\009\009return;\012\009\009#\009end if;\012\009\009end do;\012\009\009\012\009end proc:\012\012\009########################################\012\009# Initialization\012\009########################################\012\009Initialize:= proc()\012\009\009local i;\012\009\009Grad := 0; \012\009\009for i from 1 to 3 do\012\009\009\009SP(cat('CheckBox',i),'value',false);\012\009\009end do;\009\009\012\009\009SP('Grade','value', NULL);\012\009\009\012\009\009SetProblem();\012\012\009\009###################\012\009\009## Test\012\009\009###################\012\009\009if TEST = true then\012\009\009\009SP('test','visible',true);\009\012\009\009else\012\009\009\009SP('test','visible',false);\012\009\009end if;\012\009\009\012\009end proc:\012\009\012\009########################################\012\009# Input check\012\009########################################\012\009GetInput := proc()\012\009\009local UserSol, i, grad, u_cor, u_inc, sol;\012\009\009u_cor := 0; u_inc := 0;\012\009\009for i from 1 to 3 do\012\009\009\009UserSol := parse(GP(cat('CheckBox',i),'value'));\012\009\009\009sol := SOL[i];\012\009\009\009if evalb(UserSol = sol) then\012\009\009\009\009u_cor := u_cor + 1.0;\012\009\009\009else\012\009\009\009\009u_inc := u_inc + 1.0;\012\009\009\009end if;\012\009\009end do;\012\012\009\009grad := evalf[3]((u_cor)/ 3.0);\012\009\009Grad := grad;\012\009\009SP('Grade','value', Grad);\012\012\009\009#Actions:-Grade();\012\012\009end proc:\012\012\009########################################\012\009# Grade\012\009########################################\012\009Grade := proc()\012\009\009if not (Grad = 1.0) then\012\009\009\009Actions:-GetInput();\012\009\009end if;\012\009\009\009\012\009\009SP('Grade','value', Grad);\012\009\009GRADE := Grad;\012\009\009return GRADE;\012\009end proc:\012\012\009#######################################\012\009# Reset\012\009#######################################\012\012\009Reset:= proc()\012\009\009local i;\012\009\012\009\009SP('Grade','value', NULL);\012\009\009for i from 1 to 3 do\012\009\009\009SP(cat('CheckBox',i),'value',false);\012\009\009end do; \012\009\009SetProblem();\012\009end proc:\012\012\009\012########################################\009\009\012end module: # Actions module end\012########################################\012\012Actions:-Initialize():\012\012########################################"/>
