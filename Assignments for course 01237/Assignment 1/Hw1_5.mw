<Startup-Code startupcode="###########################################################\012# M\303\266bius Hjemmeopgaves\303\246t 1 til 01237 Differentialgeometri \012#  og parametrisk design\012#  Opgave 5\012# \012#  Final (version 4)\012#   - Changed TEST variable to be false.\012#   - Small design changes\012###########################################################\012restart;\012with(combinat): with(ListTools): with(LinearAlgebra): with(RandomTools):\012\012macro(\012SP = DocumentTools:-SetProperty, \012GP = DocumentTools:-GetProperty,\012DO = DocumentTools:-Do,\012RG = RandomTools:-Generate\012):\012\012Digits := 3:\012\012Actions := module()\012\009export Initialize, GetInput, SetProblem, Grade, Reset, ShowInput;\012\009global Grad, GRADE, w, SOL, TEST;\012\009########################################\012\009# Initialization\012\009########################################\012\012\009Initialize:= proc()\012\009\009local w, i;\012\009\009TEST := false;\012\009\009w := 5;\012\009\009Grad[w] := 0; \012\009\009\012\009\009SP(cat('TextArea',1+w*10),'value',NULL);\012\012\009\009for i from 1 to 5 do\012\009\009\009SP(cat('TextArea', 510 + i),'value', NULL);\012\009\009\009SP(cat('TextArea', 520 + i),'value', NULL);\012\009\009\009SP(cat('MathContainer',i),'value', NULL);\012\009\009\009SP(cat('TextArea',525+i),'value',&quot;Wrong input type&quot;);\012\009\009\009SP(cat('TextArea',525+i),'visible',false);\012\009\009end do;\012\009\009\012\009\009Actions:-SetProblem[w]();\012\012\009\009###################\012\009\009## Test\012\009\009if evalb(TEST = true) then\012\009\009\009SP(cat('test',w),'visible',true);\009\012\009\009else\012\009\009\009SP(cat('test',w),'visible',false);\012\009\009end if;\012\009\009\012\009end proc:\012\012\009########################################\012\009# Setting up the problem\012\009########################################\012\009SetProblem[5] := proc()\012\009\009local ans, T, i, j, F, p, a, b; \012\009\009w := 5;\012\009\009randomize();\012\009\009# Making random triangles \012\009\009# - note it is very unlikely for the same triangle to appear\012\009\009\012\009\009for i from 1 to 5 do\012\009\009\009p := [RG(integer(range = -5.....10)), RG(integer(range = -10.....10))];\012\009\009\009a := [RG(integer(range = -10.....15)), RG(integer(range = -5.....5))];\012\009\009\009b := [RG(integer(range = -8.....12)), RG(integer(range = -10.....5))];\012\009\009\009\012\009\009\009T[i] := [p,a,b];\012\009\009\009\012\009\009\009# Calculating the answers\012\009\009\009ans[i] := (1/2)*abs(Determinant(&lt;T[i][2][1],T[i][2][2]| T[i][3][1], T[i][3][2]&gt;));\012\009\009end do;\012\012\009\009# Writing the formulas for the triangles:\012\009\009for i from 1 to 5 do\012\009\009\009F := &quot;(&quot;;\012\009\009\009for j from 1 to 3 do\012\009\009\009\009F := cat(F,&quot;(&quot;, T[i][j][1], &quot;,&quot;, T[i][j][2], &quot;)&quot;);\012\009\009\009\009if not evalb(j = 3) then\012\009\009\009\009\009F := cat(F, &quot;,&quot;);\012\009\009\009\009end if;\012\009\009\009end do;\012\009\009\009F := cat(F, &quot;)&quot;);\012\009\009\009SP(cat('TextArea',i+510),'value',F);\012\009\009end do;\012\012\009\009# Storing the solution:\012\009\009SOL[w] := [ans[1], ans[2], ans[3] ,ans[4], ans[5]];\012\009\009\012\009\009# TEST\012\009\009SP(cat('test',w),'value',SOL[w]);\012\009\009\012\009end proc:\012\012\009########################################\012\009# Input check\012\009########################################\012\009GetInput[5] := proc()\012\009local UserSol, UserSolRaw, i, grad, grad5, soldiff, sol_crit, catch_input;\012\009\009w := 5;\012\009\009grad := 0;\012\009\009grad5 := [0, 0, 0, 0, 0];\012\009\009catch_input := [0, 0, 0, 0, 0];\012\012\009\009for i from 1 to 5 do\012\009\009\009UserSolRaw[i] := GP(cat('TextArea', i + w*100+20),'value');\012\012\009\009\009if (UserSolRaw[i] = &quot;&quot;) then\012\009\009\009\009grad5[i] := 0.0;\012\009\009\009else\012\009\009\009\009try \012\009\009\009\009\009UserSol[i] := parse(UserSolRaw[i]);\012\009\009\009\009catch :\012\009\009\009\009\009catch_input[i] := 1;\012\009\009\009\009end try;\012\012\009\009\009\009if evalb(catch_input[i] = 1) then\012\009\009\009\009\009grad5[i] := 0.0;\012\009\009\009\009\009SP(cat('TextArea',525+i),'visible',true);\012\009\009\009\009else\012\009\009\009\009\009if not (whattype(evalf(UserSol[i])) = float) then\012\009\009\009\009\009\009SP(cat('TextArea',525+i),'visible',true);\012\009\009\009\009\009\009grad5[i] := 0.0;\012\009\009\009\009\009else\012\009\009\009\009\009\009SP(cat('TextArea',525+i),'visible',false);\012\009\009\009\009\009\009soldiff := evalf(abs(UserSol[i]-SOL[w][i]));\012\009\009\009\009\009\009sol_crit := 0.1*evalf(SOL[w][i]);\012\009\009\009\009\009\009\012\009\009\009\009\009\009if soldiff = 0 then \012\009\009\009\009\009\009\009grad5[i] := 1.0;\012\009\009\009\009\009\009elif soldiff &lt;= sol_crit then\012\009\009\009\009\009\009\009grad5[i] := 0.5;\012\009\009\009\009\009\009else \012\009\009\009\009\009\009\009grad5[i] := 0;\012\009\009\009\009\009\009end if;\012\009\009\009\009\009end if;\012\009\009\009\009end if;\012\009\009\009end if;\012\009\009end do;\012\012\009\009grad := evalf( (grad5[1]+grad5[2]+grad5[3]+grad5[4]+grad5[5])/5.0);\012\009\009\012\009\009\012\009\009SP(cat('TextArea',1+10*w),'value',grad);\012\012\009\009Grad[w] := grad;\012\009\009#Actions:-Grade();\012\012\009end proc:\012\012\009#######################################\012\009# Reset\012\009#######################################\012\009Reset[5]:= proc()\012\009\009local w, i;\012\009\009w := 5;\012\009\009# Resetting the answer boxes to be empty\012\009\009SP(cat('TextArea',1+w*10),'value', NULL);\012\009\009for i from 1 to 5 do\012\009\009\009SP(cat('TextArea',520+i),'value',NULL);\012\009\009\009SP(cat('MathContainer',i),'value',NULL);\012\009\009\009SP(cat('TextArea',525+i),'visible',false);\012\009\009end do;\012\009\009Actions:-SetProblem[w]();\012\009\009\012\009end proc:\012\012\009#######################################\012\009# Grade\012\009#######################################\009\012\009Grade := proc()\012\009\009local w;\012\009\009w := 5;\012\009\009if not (Grad[w] = 1.0) then\012\009\009\009Actions:-GetInput[w]();\012\009\009\009GRADE := Grad[w];\012\009\009end if;\012\009\009GRADE := Grad[w];\012\012\009end proc:\012\012\009#######################################\012\009# Show Input\012\009#######################################\012\009ShowInput := proc( n )\012\009\009local uRaw, UserSol, catch_input;\012\009\009#w := 5;\012\009\009catch_input := 0;\012\009\009uRaw := GP(cat('TextArea',n+520),'value');\012\012\009\009if not (uRaw = &quot;&quot;) then\012\009\009\009try \012\009\009\009\009UserSol := parse(uRaw);\012\009\009\009catch :\012\009\009\009\009catch_input := 1;\012\009\009\009end try;\012\009\012\009\009\009if (catch_input = 1) then\012\009\009\009\009SP(cat('TextArea',525+n),'visible',true);\012\009\009\009\009SP(cat('MathContainer',n),'value', Error);\012\009\009\009else\012\009\009\009\009if not (whattype(evalf(UserSol)) = float) then\012\009\009\009\009\009#SP(cat('TextArea',525+n),'visible',true);\012\009\009\009\009\009SP(cat('MathContainer',n),'value', UserSol);\012\009\009\009\009else\012\009\009\009\009\009SP(cat('MathContainer',n),'value',UserSol);\012\009\009\009\009end if;\012\009\009\009end if;\012\009\009end if;\012\009\009\012\009end proc:\012\012\012end module:\012\012Actions:-Initialize();"/>
