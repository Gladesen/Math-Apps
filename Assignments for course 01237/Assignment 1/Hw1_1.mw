<Startup-Code startupcode="###########################################################\012# M\303\266bius Hjemmeopgaves\303\246t 1 til 01237 Differentialgeometri \012#  og parametrisk design\012#\009\009\009\009Opgave 1\012#\012#  Final (version 5)\012#   - Changed TEST variable to be false.\012#   - Small design changes\012###########################################################\012restart;\012with(combinat): with(ListTools):\012\012macro(\012SP = DocumentTools:-SetProperty, \012GP = DocumentTools:-GetProperty,\012RG = DocumentTools:-Generate\012):\012\012Digits := 3:\012\012Actions := module()\012\009export Initialize, GetInput, SetProblem, Grade, Reset;\012\009global Grad, GRADE, SOL, TEST;\012\009local a, b, c, s, h, g, w;\012\012\009########################################\012\009# Initialization\012\009########################################\012\009\012\009Initialize:= proc()\012\009\009local w, i, comboList;\012\009\009TEST := false;\012\009\009w := 1;\012\009\009Grad[w] := 0; \012\012\009\009SP(cat('TextArea',1+w*10),'value',NULL);\012\012\009\009comboList := [&quot;--&quot;,&quot;1&quot;, &quot;2&quot;, &quot;3&quot;, &quot;4&quot;, &quot;5&quot;];\012\009\009for i from 1 to 5 do\012\009\009\009SP(cat('ComboBox', i+w*10), 'itemlist', comboList);\012\009\009\009SP(cat('ComboBox', i+w*10), 'value', &quot;--&quot;);\012\009\009end do;\012\009\009\012\009\009Actions:-SetProblem[w]();\012\009\009\012\009\009###################\012\009\009## Test\012\009\009###################\012\009\009if evalb(TEST = true) then\012\009\009\009SP(cat('test',w),'visible',true);\012\009\009else\012\009\009\009SP(cat('test',w),'visible',false);\012\009\009end if;\012\009\009\012\009end proc:\012\009\012\009########################################\012\009# Setting up the problem\012\009########################################\012\009SetProblem[1] := proc()\012\009\009local r, i, sol, Prob1_List, Pyth, Area_abc, \012\009\009Reg_triangle, Angle, Area_triangle;\012\009\009sol := [0, 0, 0, 0, 0];\012\009\009w := 1;\012\012\009\009randomize();\012\009\009\012\009\009# We want a random permutation in order for the answer\012\009\009# to not be the same every time:\012\009\009Prob1_List := randperm([1,2,3,4,5]);\012\012\009\009\012\009\009# Options for the question\012\009\009Pyth := a^2+b^2 = c^2;\012\009\009Area_abc := 'A' = sqrt(s*(s-a)*(s-b)*(s-c));\012\009\009Reg_triangle := 'A'&gt;0;\012\009\009Angle := Pi;\012\009\009Area_triangle := (1/2)*h*g;\012\009\009\012\009\009r := ([Pyth, Area_abc, Reg_triangle, Angle, Area_triangle]);\012\012\009\009# Writing the options in the found random order\012\009\009for i from 1 to 5 do\012\009\009\009SP(cat('MathContainer', i+10), 'value', r[Prob1_List[i]]);\012\009\009\009sol[i] := Search(i, Prob1_List);\012\009\009end do;\012\012\009\009# Storing the solution in a global variable:\012\009\009SOL[w] := sol;\012\009\009\012\009\009# TEST\012\009\009SP(cat('test',w),'value',sol);\012\009\009\012\009end proc:\012\012\009########################################\012\009# Input check\012\009########################################\012\009GetInput[1] := proc()\012\009\009local UserSol, c, i, grad;\012\009\009w := 1;\012\009\009c := 0;\012\009\009\012\009\009for i from 1 to 5 do\012\009\009\009if not (GP(cat('ComboBox', i + w*10),'value') = &quot;--&quot;) then\012\009\009\009\009UserSol[i] := parse(GP(cat('ComboBox', i + w*10),'value'));\012\009\009\009\009if evalb(evalf(UserSol[i]) = evalf(SOL[w][i])) then\012\009\009\009\009\009c := c + 1;\012\009\009\009\009end if;\012\009\009\009end if;\012\009\009end do;\012\009\009\012\009\009grad := evalf(c/5);\012\009\009SP(cat('TextArea',1+10*w),'value',grad);\012\012\009\009Grad[w] := grad;\012\009\009\012\009\009\012\009end proc:\012\012\009#######################################\012\009# Reset\012\009#######################################\012\009Reset[1]:= proc()\012\009\009local w, i, comboList;\012\009\009w := 1;\012\009\009# First setting all text areas and math containers etc. to NULL\012\009\009\012\009\009SP(cat('TextArea',1+w*10),'value', NULL);\012\009\009\012\009\009comboList := [&quot;--&quot;,&quot;1&quot;, &quot;2&quot;, &quot;3&quot;, &quot;4&quot;, &quot;5&quot;];\012\009\009for i from 1 to 5 do\012\009\009\009SP(cat('ComboBox', i+10), 'itemlist', comboList);\012\009\009\009SP(cat('ComboBox', i+10), 'value', &quot;--&quot;);\012\009\009end do;\012\012\009\009Actions:-SetProblem[w]();\012\009\009\012\009end proc:\012\012\009########################################\012\009# Grade\012\009########################################\012\009Grade := proc()\012\009\009local w;\012\009\009w := 1;\012\009\009if not evalb(Grad[w] = 1.0) then\012\009\009\009Actions:-GetInput[w]();\012\009\009end if;\012\009\009GRADE := Grad[w];\012\009\012\009end proc:\012\009\012end module: # Actions module end\012############################################\012\012Actions:-Initialize();\012"/>
