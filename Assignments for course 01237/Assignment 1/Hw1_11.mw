<Startup-Code startupcode="###########################################################\012# M\303\266bius Hjemmeopgaves\303\246t 1 til 01237 Differentialgeometri \012#  og parametrisk design\012#\009\009\009\009Opgave 11\012#        - Original af Steen Markvorsen\012# Final (version 3)\012#   - Changed TEST variable to be false.\012#   - Small design changes\012##########################################################\012restart;\012with(RandomTools): with(plots): with(plottools):\012\012macro(\012SP = DocumentTools:-SetProperty, \012GP = DocumentTools:-GetProperty,\012RG = RandomTools:-Generate\012):\012\012Digits := 3:\012\012##########################################################\012Actions := module()\012\009export Initialize, Plot, GetInput, Grade, SetProblem, ShowInput, Reset;\012\009local triang;\012\009global Grad, SOL, GRADE, GO;\012\012\009########################################\012\009# Initialization\012\009########################################\012\009Initialize := proc()\012\009\009local i;\012\009\009GO := 0; SOL := 0; Grad := 0.0;\012\009\009\012\009\009for i from 1 to 3 do\012\009\009\009SP(cat('TextArea',i),'value',NULL);\012\009\009\009SP(cat('MathContainer',i+1),'value',NULL);\012\009\009end do;\012\009\009SP('Comment','value', NULL);\012\009\009SP('Grade','value', NULL);\012\009\009SP('Plot0','value',NULL);\012\009\009SP(cat('MathContainer',1),'value',NULL);\012\009\009\012\009\009Actions:-SetProblem();\012\009\009Actions:-Plot();\012\009\009\012\009\009\012\009end proc:\012\012\009########################################\012\009# Setting up the problem\012\009########################################\012\009SetProblem := proc()\012\009\009randomize();\012\009\009\012\009\009# Generating the wanted area\012\009\009SOL := RG(integer(range = 1..100));\012\009\009SP(cat('MathContainer',1),'value', SOL);\012\009\009\012\009end proc:\012\012\009########################################\012\009# Input check\012\009########################################\012\009GetInput := proc()\012\009\009local a, b, c, aRaw, bRaw, cRaw, catch_input, s, grad, Area;\012\009\009catch_input := 0;\012\009\009grad := 0.0;\012\012\009\009Area := 0;\012\009\009\012\009\009aRaw := GP('TextArea1','value');\012\009\009bRaw := GP('TextArea2','value');\012\009\009cRaw := GP('TextArea3','value');\012\012\009\009if (aRaw = &quot;&quot;) or (bRaw = &quot;&quot;) or (cRaw = &quot;&quot;) then\012\009\009\009GO := 0;\012\009\009\009grad := 0.0;\012\009\009\009SP('Comment','value', &quot;Inds\303\246t l\303\246ngderne i felterne ovenfor&quot;);\012\009\009else\012\009\009\009try \012\009\009\009\009a := parse(aRaw);\012\009\009\009\009b := parse(bRaw);\012\009\009\009\009c := parse(cRaw);\012\009\009\009catch :\012\009\009\009\009catch_input := 1;\012\009\009\009end try;\012\012\009\009\009if catch_input = 1 then\012\009\009\009\009GO := 0;\012\009\009\009\009grad := 0.0;\012\009\009\009\009SP('Comment','value', &quot;Der skete en fejl. Indtast input som reelle tal.&quot;);\012\009\009\009else\012\009\009\009\009if not(whattype(evalf(a)) = float) or not(whattype(evalf(b)) = float) or not(whattype(evalf(c))=float) then\012\009\009\009\009\009SP('Comment','value', &quot;Forkert type input. Inds\303\246t reelle tal.&quot;);\012\009\009\009\009\009GO := 1;\012\009\009\009\009\009grad := 0.0;\012\009\009\009\009elif evalb(evalf(min(a,b,c))&lt;=0) then\012\009\009\009\009\009GO := 0;\012\009\009\009\009\009grad := 0.0;\012\009\009\009\009\009SP('Comment','value',&quot;Alle sidel\303\246ngder skal v\303\246re positive.&quot;);\012\009\009\009\009else\012\009\009\009\009\009s := (1/2)*(a+b+c);\012\009\009\009\009\009\012\009\009\009\009\009\012\009\009\009\009\009if evalf(s) &gt; evalf(max(a,b,c)) then\012\009\009\009\009\009\009GO := 1;\012\009\009\009\009\009\009Area := simplify(sqrt(s*(s-a)*(s-b)*(s-c)));\012\009\009\009\009\009\009SP('Comment','value', cat(&quot;Arealet er &quot;, evalf(Area)));\012\009\009\009\009\009\009if evalb( abs(evalf(Area-SOL)) &lt; evalf(0.01*SOL)) then\012\009\009\009\009\009\009\009grad := 1.0;\012\009\009\009\009\009\009end if;\012\009\009\009\009\009\009\012\009\009\009\009\009else\012\009\009\009\009\009\009GO := 0;\012\009\009\009\009\009\009grad := 0.0;\012\009\009\009\009\009\009SP('Comment','value', cat(&quot;Disse sidel\303\246ngder udg\303\270r ikke en regul\303\246r trekant.&quot;));\012\009\009\009\009\009end if;\012\009\009\009\009end if;\012\009\009\009end if;\012\009\009end if;\012\009\009\012\009\009Grad := grad;\012\009\009SP('Grade','value',Grad);\012\012\009\009Actions:-Plot();\012\009\009\012\009end proc: # end input\012\012\009########################################\012\009# Creating the plot\012\009########################################\012\009Plot := proc()\012\009\009local LL, L, Sgam, Cgam, Sbeta, Cbeta, triangFill, a,b,c, s, Area;\012\012\009\009if evalb(GO = 1) then\012\009\009\009a := evalf(parse(GP('TextArea1','value')));\012\009\009\009b := evalf(parse(GP('TextArea2','value')));\012\009\009\009c := evalf(parse(GP('TextArea3','value')));\012\012\009\009\009s := (1/2)*(a+b+c);\012\009\009\009Area := simplify(sqrt(s*(s-a)*(s-b)*(s-c)));\012\012\009\009\009\012\009\009\009LL:= sort([a,b,c], `&gt;`);\012\009\009\009L:= [1, evalf(LL[2]/LL[1]), evalf(LL[3]/LL[1])];\012\009\009\009Sgam:= 2*Area/(LL[2]*LL[1]);\012\009\009\009Cgam:= (LL[1]^2 + LL[2]^2 - LL[3]^2)/(2*LL[1]*LL[2]);\012\009\009\009Sbeta:= 2*Area/(LL[3]*LL[1]);\012\009\009\009Cbeta:= (LL[1]^2 + LL[3]^2 - LL[2]^2)/(2*LL[1]*LL[3]);\012\009\009\009triang:= (\012\009\009\009\009line([0,0], [1,0], thickness=3, color=blue), \012\009\009\009\009line([0,0], L[2]*[Cgam, Sgam], thickness=3, color=blue), \012\009\009\009\009line([1,0], [1,0]+L[3]*[-Cbeta, Sbeta], thickness=3, color=blue), \012\009\009\009\009disk([0,0], 0.05, color=red),\012\009\009\009\009disk([1,0], 0.05, color=red),\012\009\009\009\009disk([1,0]+L[3]*[-Cbeta, Sbeta], 0.05, color=red)\012\009\009\009);\012\009\009\009triangFill:= polygon([[0,0], [1,0], [1,0]+L[3]*[-Cbeta, Sbeta]], color=cyan);\012\009\009\009SP('Plot0', 'value', plots:-display(triang, triangFill, axes=none, scaling=constrained));\012\009\009else\012\009\009\009SP('Plot0','value',plots:-display(plots:-pointplot([]), tickmarks=[0,0]));\012\009\009end if;\012\009\009\012\009end proc:\012\009\012\009########################################\012\009# Show input\012\009########################################\012\009ShowInput := proc( n )\012\009\009local xRaw, x, catch_input;\012\009\009xRaw := GP(cat('TextArea',n),'value');\012\009\009catch_input := 0;\012\009\009\012\009\009if  not(xRaw = &quot;&quot;) then\012\009\009\009try \012\009\009\009\009x := parse(xRaw);\012\009\009\009catch :\012\009\009\009\009catch_input := 1;\012\009\009\009end try;\012\009\009\009\012\009\009\009if (catch_input = 1) then\012\009\009\009\009SP(cat('MathContainer',n+1),'value',Error);\012\009\009\009else\012\009\009\009\009SP(cat('MathContainer',n+1),'value',x);\012\009\009\009end if;\012\009\009end if;\012\009\009\012\009end proc:\012\009\012\009########################################\012\009# Grading\012\009########################################\012\009Grade := proc()\012\009\009if not(Grad  = 1.0) then\012\009\009\009Actions:-GetInput();\012\009\009end;\012\009\009GRADE := Grad;\012\009end proc:\012\012\009########################################\012\009# Reset\012\009########################################\012\009Reset := proc()\012\009\009local i;\012\009\009for i from 1 to 3 do\012\009\009\009SP(cat('TextArea',i),'value',NULL);\012\009\009\009SP(cat('MathContainer',i+1),'value',NULL);\012\009\009end do;\012\009\009SP('Comment','value', NULL);\012\009\009SP('Grade','value', NULL);\012\009\009SP('Plot0','value',NULL);\012\009\009SP(cat('MathContainer',1),'value',NULL);\012\009\009GO := 0;\012\009\009Actions:-SetProblem();\012\009\009Actions:-Plot();\012\009end proc:\012\012end module:\012\012########################################################