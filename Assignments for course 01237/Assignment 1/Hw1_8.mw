<Startup-Code startupcode="###########################################################\012# M\303\266bius Hjemmeopgaves\303\246t 1 til 01237 Differentialgeometri \012#  og parametrisk design\012#\009\009\009\009Opgave 8\012#  Final (version 4)\012#   - Changed TEST variable to be false.\012#   - Small design changes\012##########################################################\012restart;\012with(LinearAlgebra): with(RandomTools):\012\012macro(\012SP = DocumentTools:-SetProperty, \012GP = DocumentTools:-GetProperty,\012RG = RandomTools:-Generate\012):\012\012Digits := 3:\012\012Actions := module()\012\009export Initialize, GetInput, SetProblem, Grade, Reset, ShowInput;\012\009global Grad, GRADE, w, SOL, TEST, Error;\012\009########################################\012\009# Initialization\012\009########################################\012\012\009Initialize:= proc()\012\009\009local w;\012\009\009TEST := false;\012\009\009w := 8;\012\009\009Grad[w] := 0; \012\009\009\012\009\009SP(cat('TextArea',1+w*10),'value',NULL);\012\009\009SP(cat('TextArea',2+w*10),'value',NULL);\012\009\009SP(cat('MathContainer',9), 'value', NULL);\012\009\009SP(cat('TextArea',4+w*10),'value', &quot;Wrong input type&quot;);\012\009\009SP(cat('TextArea',4+w*10),'visible', false);\012\009\009\012\009\009Actions:-SetProblem[w]();\012\012\009\009###################\012\009\009## Test\012\009\009if evalb(TEST = true) then\012\009\009\009SP(cat('test',w),'visible',true);\009\009\009\012\009\009else\012\009\009\009SP(cat('test',w),'visible',false);\012\009\009end if;\012\009\009\012\009end proc:\012\012\009########################################\012\009# Setting up the problem\012\009########################################\012\009SetProblem[8] := proc()\012\009\009local ans, K, T, M, F, i; \012\009\009w := 8;\012\009\009randomize();\012\009\009K := Generate('Matrix'(integer(range = -1 .. 5), 2, 2));\012\009\009T := Generate('Matrix'(integer(range = -1 .. 5), 3, 2));\012\009\009\012\009\009# Writing the triangles on the wanted form\012\009\009F := &quot;(&quot;;\012\009\009for i from 1 to 3 do\012\009\009\009F := cat(F,&quot;(&quot;, T[i][1], &quot;,&quot;, T[i][2], &quot;)&quot;);\012\009\009\009if not evalb(i = 3) then\012\009\009\009\009F := cat(F, &quot;,&quot;);\012\009\009\009end if;\012\009\009end do;\012\009\009F := cat(F, &quot;)&quot;);\012\009\009SP(cat('TextArea',3+w*10),'value',F);\012\009\009SP(cat('MathContainer',w),'value',K);\012\009\009\012\009\009M := K.&lt;&lt;T[2][1], T[2][2]&gt; | &lt;T[3][1], T[3][2]&gt;&gt;; # determining new vectors\012\009\009\012\009\009# Finally we can write the new triangle: \012\009\009ans := [[T[1][1],T[1][2]], [M[1][1],M[2][1]],[M[1][2],M[2][2]]];\012\009\009\012\009\009SOL[w] := ans;\012\009\009# TEST\012\009\009SP(cat('test',w),'value',ans);\012\009end proc:\012\012\009########################################\012\009# Input check\012\009########################################\012\009GetInput[8] := proc()\012\009\009local UserSol, uRaw, i, j, grad, catch_input; \012\009\009w := 8;\012\009\009catch_input := 0; grad := 0.0;\012\009\009uRaw := GP(cat('TextArea',2+80),'value');\012\012\009\009if (uRaw = &quot;&quot;) then\012\009\009\009grad := 0.0;\012\009\009else\012\009\009\009try\012\009\009\009\009UserSol := parse(uRaw);\012\009\009\009catch :\012\009\009\009\009catch_input := 1;\012\009\009\009end try;\012\012\009\009\009if evalb(catch_input = 1) then\012\009\009\009\009grad := 0.0;\012\009\009\009\009SP('MathContainer9','value',Error);\012\009\009\009else\012\009\009\009\009SP(cat('TextArea',4+w*10),'visible', false);\012\009\009\009\009for i from 1 to 3 do\012\009\009\009\009\009for j from 1 to 2 do\012\009\009\009\009\009\009if evalb(UserSol[i][j] = SOL[w][i][j]) then\012\009\009\009\009\009\009\009grad := grad + 1;\012\009\009\009\009\009\009end if;\012\009\009\009\009\009end do;\012\009\009\009\009end do;\012\009\009\009end if;\012\009\009end if;\012\009\009grad := grad / 6.0;\012\012\009\009SP(cat('TextArea',1+w*10),'value',grad);\012\009\009Grad[w]:= grad;\012\009\009\012\009\009\012\009end proc:\012\012\009#######################################\012\009# Reset\012\009#######################################\012\009Reset[8]:= proc()\012\009\009local w;\012\009\009w := 8;\012\009\009# Resetting the boxes to be empty\012\009\009SP(cat('TextArea',2+w*10),'value', NULL);\012\009\009SP(cat('TextArea',1+w*10), 'value',NULL);\012\009\009SP(cat('MathContainer',9),'value',NULL);\012\009\009SP(cat('TextArea',4+w*10),'visible', false);\012\009\009Actions:-SetProblem[w]();\012\009\009\012\009end proc:\012\012\009#######################################\012\009# Grade\012\009#######################################\009\012\009Grade := proc()\012\009\009w := 8;\012\009\009if not (Grad[w] = 1.0) then\012\009\009\009Actions:-GetInput[w]();\012\009\009end if;\012\009\009GRADE := Grad[w];\012\012\009end proc:\012\009\012\009#######################################\012\009# Show Input\012\009# Adds the possibility for the student to see how their answer is interpreted in the MathApp.\012\009#######################################\012\009ShowInput := proc()\012\009\009local uRaw, UserSol, catch_input;\012\009\009w := 8;\012\009\009uRaw := GP(cat('TextArea',2+w*10),'value');\012\012\009\009if not (uRaw = &quot;&quot;) then\012\009\009\009try\012\009\009\009\009UserSol := parse(uRaw);\012\009\009\009catch :\012\009\009\009\009catch_input := 1;\012\009\009\009end try;\012\012\009\009\009if (catch_input = 1) then\012\009\009\009\009SP(cat('MathContainer',9),'value', Error);\012\009\009\009\009SP(cat('TextArea',4+w*10),'visible', true);\012\009\009\009else\012\009\009\009\009SP(cat('MathContainer',9),'value', UserSol);\012\009\009\009end if;\012\009\009end if;\012\009\009\012\009end proc:\012\012end module:\012\012Actions:-Initialize();"/>
