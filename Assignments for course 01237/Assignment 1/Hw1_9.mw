<Startup-Code startupcode="###########################################################\012# Hjemmeopgave 1  01237 Differential Geometry and Parametric design\012#     Opgave 9\012#      - Af Steen Markvorsen, 26.08.22\012#\012#  Final (version 3)\012#   - Changed TEST variable to be false.\012#   - Small design changes\012###########################################################\012restart;\012with(combinat): with(ListTools):with(LinearAlgebra):with(RandomTools):\012with(plots): with(plottools):\012\012macro(\012SP = DocumentTools:-SetProperty, \012GP = DocumentTools:-GetProperty,\012RG = RandomTools:-Generate\012):\012\012Digits := 3:\012\012\012############################################################\012# ACTIONS\012############################################################\012Actions := module()\012\009export Initialize, GetInput, SetProblem, Grade, Reset;\012#\009local w;\012\009global SOLA, SOLR, GradA, GradR, GradTotal, TEST, GRADE;\012\012\009#####################\012\009TEST := false;\012\009#####################\012\012\009############################################################\012\009# SETPROBLEM\012\009############################################################\009\012\009SetProblem := proc()\012\009\009local ran, ran2, Atest, p, q, r, triang, triangFill, \012\009\009Lengths, Rad1, a1, outdist, deltaout, SOL, lin1, lin2, C, dotTest, w, siz, C1 ;\012\009\009global u, t;\012\012\009\009# GENERATING RANDOM TRIANGLE WITH POSITIVE AREA:\012\009\009randomize(); \012\009\009   \012\009\009ran:= rand(-10...10);\012\009\009ran2:= rand(1...4);\012\009\009Atest:= true; dotTest:= true; \012\009\009for w from 1 by 1 to 50 while Atest or dotTest do \009\012\009\009\009p :=  [ran(), ran()];\012\009\009\009q :=  [ran(), ran()];\012\009\009#\009r :=  [ran(), ran()]; \012\009\009\009\012\009\009\009dotTest:= evalb(evalf(DotProduct((p-q), (p-q))) &lt;5.0);\012\009\009\009r:= map(floor, convert(evalm(0.5*ran2()*(p+q) + 0.8*ran2()*[-(q-p)[2], (q-p)[1]]), list));\012\009\009\009Atest:= evalb( evalf(0.5*abs(Determinant(Matrix([q-p, r-p])))) &lt; 25.0);\012\009\009end do;\012\009\009\012\009\009# ROUGH SIZE OF THE TRIANGLE:\012\009\009siz := max(map(op, [p,q,r])) - min(map(op, [p,q,r]));\012\009\009\012\009\009# PRESENTING THE POINTS:\012\009\009\009SP(cat('TextArea_p'),'value', p);\012\009\009\009SP(cat('TextArea_q'),'value', q);\012\009\009\009SP(cat('TextArea_r'),'value', r);\012\009\009\012\009\009# FINDING THE AREA:\012\009\009SOLA := (1/2)*abs(Determinant(Matrix([q-p, r-p])));\009\012\009\009\012\009\009# Storing the area solution\012\009\009SP('testA','value', (SOLA));\012\009\009\012\009\009# FINDING THE SMALLEST CONTAINING DISK:\012\009\009Lengths := sort([[DotProduct(r-p, r-p), 1], [DotProduct(q-p, q-p), 2], [DotProduct(r-q, r-q), 3]]);\012\009\009Rad1 := (1/2)*sqrt(Lengths[3][1]);\012\009\009for w from 1 to 3 do \012\009\009\009if Lengths[3][2]=1 then \012\009\009\009\009C1:= (1/2)*(r+p); \012\009\009\009\009a1:= q;\012\009\009\009elif Lengths[3][2]=2 then \012\009\009\009\009C1:= (1/2)*(q+p);  \012\009\009\009\009a1:= r;\012\009\009\009elif Lengths[3][2]=3 then \012\009\009\009\009C1:= (1/2)*(q+r); \012\009\009\009\009a1:= p;\012\009\009\009end if;\012\009\009end do:\012\009\009\012\009\009outdist:= sqrt(DotProduct(a1-C1, a1 -C1));\012\009\009deltaout:= evalf(Rad1 - outdist);\012\009\009\012\009\009if deltaout &gt; 0.0 then \012\009\009\009C:= C1; \012\009\009\009SOLR:= Rad1; \012\009\009else\012\009\009\009lin1 := u-&gt; convert(evalm((1/2)*(p+q) + u*[-(q-p)[2], (q-p)[1]]), list):\012\009\009\009lin2 := t-&gt; convert(evalm((1/2)*(p+r) + t*[-(r-p)[2], (r-p)[1]]), list):\012\009\009\009SOL := solve({lin1(u)[1]= lin2(t)[1], lin1(u)[2]=lin2(t)[2]}, [u,t]):\012\009\009\009C := lin1(rhs(SOL[1][1])):\012\009\009\009SOLR := sqrt(DotProduct(p-C, p-C)):\012\009\009end if;\012\009\009\012\009\009# Storing the radius solution\012\009\009SP('testR','value', evalf(SOLR));\012\009\009\012\009\009# PLOTTING THE TRIANGLE AND DISK:\012\009\009\009triang:= (\012\009\009\009\009line(p,q, thickness=3, color=blue), \012\009\009\009\009line(q,r, thickness=3, color=blue), \012\009\009\009\009line(p,r, thickness=3, color=blue), \012\009\009\009\009disk(p, 0.03*siz, color=red),\012\009\009\009\009disk(q, 0.03*siz, color=red),\012\009\009\009\009disk(r, 0.03*siz, color=red),\012\009\009          circle(C, SOLR, color=red)\012\009\009\009\009):\012\009\009\009triangFill:= polygon([p,q,r], color=cyan):\012\009\009\009SP('Plot0', 'value', plots:-display(triang, triangFill, gridlines, scaling=constrained)):\012\009\009\009\012\009end proc: # end SetProblem\012\012\009########################################\012\009# INITIALIZATION\012\009########################################\012\009Initialize:= proc();\012\012\009\009GradA := 0.0; GradR:= 0.0;\012\009\009SP('InputA','value', NULL);\012\009\009SP('InputR','value', NULL);\012\009\009SP('GradeA','value', NULL);\012\009\009SP('GradeR','value', NULL);\012\009\009SP('GradeTotal','value', NULL);\012\009\009SP('MathContainer2','value',NULL);\012\009     SP('MathContainer3','value',NULL);\012\009\009SP('Input1','value',&quot;Wrong input type&quot;);\012\009\009SP('Input2','value',&quot;Wrong input type&quot;);\012\009\009SP('Input1','visible',false);\012\009     SP('Input2','visible',false);\012\009     \012\009\009SetProblem();\012\012\009\009###################\012\009\009## Test\012\009\009###################\012\009\009if TEST = false then\012\009\009\009SP('testA','visible',false);\009\012\009\009\009SP('testR','visible',false);\012\009\009else\012\009\009\009SP('testA','visible',true);\009\012\009\009\009SP('testR','visible',true);\012\009\009end if;\012\009\009\012\009end proc:\012\009\012\009########################################\012\009# GET INPUT AND CHECK AND GRADE\012\009########################################\012\009GetInput := proc()\012\009\009local UserSolA, UserSolRawA, gradA,  UserSolR, UserSolRawR, gradR,\012\009\009soldiffA, solcritA, soldiffR, solcritR, gradTotal, catch_input;\012\009\009\012\009\009SP('Input1','visible',false);\012\009\009SP('Input2','visible',false);\012\009\009\012\009\009##### Area of the triangle\012\009\009catch_input := 0;\012          UserSolRawA := GP('InputA','value');\012\012\009\009if (UserSolRawA = &quot;&quot;) then\012\009\009\009gradA := 0.0;\012\009\009else\012\009\009\009try \012\009\009\009\009UserSolA := parse(UserSolRawA);\012\009\009\009catch :\012\009\009\009\009catch_input := 1;\012\009\009\009end try;\012\009\009\009\012\009\009\009if evalb(catch_input = 1) then\012\009\009\009\009gradA := 0.0;\012\009\009\009\009SP('MathContainer2','value', Error);\012\009\009\009\009SP('Input1','visible',true);\012\009\009\009elif not(whattype(evalf(UserSolA)) = float) then\012\009\009\009\009gradA := 0.0;\012\009\009\009\009SP('MathContainer2','value', UserSolA);\012\009\009\009\009SP('Input1','visible',true);\012\009\009\009else\012\009\009\009\009SP('MathContainer2','value', UserSolA);\012\009\009\009\009\012\009\009\009\009soldiffA := abs(evalf(UserSolA - SOLA));\012\009\009\009\009solcritA := evalf(0.1*SOLA);\012\009\012\009\009\009\009if soldiffA = 0 then\012\009\009\009\009\009gradA := 1.0;\012\009\009\009\009elif soldiffA &lt;= solcritA then\012\009\009\009\009\009gradA := 0.5;\012\009\009\009\009else\012\009\009\009\009\009gradA := 0.0;\012\009\009\009\009end if;\012\009     \009end if;\012\009     end if;\012\012\009\009SP('GradeA','value',gradA);\012\009\009GradA := gradA;\012\012\009\009##### Radius of the smallest disk\012\009     catch_input := 0;\012         \009UserSolRawR:=  GP('InputR','value');\012\012\009\009if (UserSolRawR = &quot;&quot;) then\012\009\009\009gradR := 0.0;\012\009\009else\012\009\009\009try \012\009\009\009\009UserSolR := parse(UserSolRawR);\012\009\009\009catch :\012\009\009\009\009catch_input := 1;\012\009\009\009end try;\012\012\009\009\009if evalb(catch_input = 1) then\012\009\009\009\009gradR := 0.0;\012\009\009\009\009SP('MathContainer3','value', Error);\012\009\009\009\009SP('Input2','visible',true);\012\009\009\009elif not(whattype(evalf(UserSolR)) = float) then \012\009\009\009\009gradR:= 0.0;\012\009\009\009\009SP('MathContainer3','value', UserSolR);\012\009\009\009\009SP('Input2','visible',true);\012\009\009\009else \012\009\009\009\009SP('MathContainer3','value', UserSolR);\012\012\009\009\009\009soldiffR := abs(evalf(UserSolR - SOLR));\012\009\009\009\009solcritR := evalf(0.1*SOLR);\012\012\009\009\009\009if soldiffR = 0 then\012\009\009\009\009\009gradR := 1.0;\012\009\009\009\009elif soldiffR &lt;= solcritR then\012\009\009\009\009\009gradR := 0.5;\012\009\009\009\009else\012\009\009\009\009\009gradR := 0.0;\012\009\009\009\009end if;\012\009\009\009end if;\012\009\009end if;\012\009\009\012\009\009SP('GradeR','value',gradR);\012\009\009GradR := gradR;\012\012          gradTotal:= 0.5*(GradA + GradR);\012          GradTotal:= gradTotal;\012\012          SP('GradeA','value', GradA);\012\009\009SP('GradeR','value', GradR);\012\009\009SP('GradeTotal','value', GradTotal);\012\012\009     \012\009end proc:\012\012\009########################################\012\009# GRADE OUT\012\009########################################\012\009Grade := proc();\012\009\009if not(Grad  = 1.0) then\012\009\009\009Actions:-GetInput();\012\009\009end;\012\009\009GRADE := GradTotal;\012\009end proc:\012\012\009#######################################\012\009# RESET\012\009#######################################\012\009Reset:= proc()\012\009\009SP('InputA','value', NULL);\012\009\009SP('InputR','value', NULL);\012\012\009\009SP('GradeA','value', NULL);\012\009\009SP('GradeR','value', NULL);\012\009\009SP('GradeTotal','value', NULL);\012\009\009SP('MathContainer2','value',NULL);\012\009     SP('MathContainer3','value',NULL);\012\012\009     SP('Input1','visible',false);\012\009     SP('Input2','visible',false);\012\009\009\012\009\009SetProblem();\012\009\009\012\009end proc:\012\009\012####################