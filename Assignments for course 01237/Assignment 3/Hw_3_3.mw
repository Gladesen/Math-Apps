<Startup-Code startupcode="###########################################################\012# M\303\266bius Hjemmeopgaves\303\246t 3 til 01237 Differentialgeometri \012#  og parametrisk design\012#  Opgave 4: Helix\012#     - Line Glade, 16/09/22\012# Version 2 (by Steen Markvorsen)\012# - Animation didn't work in M\303\266bius - changed to be a static image\012###########################################################\012restart;\012with(RandomTools): with(LinearAlgebra): with(ListTools, Search): with(combinat): with(ArrayTools): with(plots): with(plottools):\012\012macro(\012SP = DocumentTools:-SetProperty, \012GP = DocumentTools:-GetProperty,\012RG = RandomTools:-Generate\012):\012\012Actions := module()\012\009export Initialize, GetInput, SetProblem, Grade, Reset;\012\009local SOL;\012\009global Grad, TEST;\012\012\009# Test variable: Set to false before export\012\009TEST := false;\012\012\009########################################\012\009# Initialization\012\009########################################\012\009Initialize:= proc()\012\009\009local i;\012\009\009Grad := 0;\012\009\009for i from 1 to 6 do\012\009\009\009SP(cat('RadioButton',i),'value', false); # Resetting radio buttons\012\009\009end do;\012\009\009SP('Grade','value',NULL);\012\012\009\009Actions:-SetProblem();\012\009\009\012\009\009## Test\012\009\009if evalb(TEST = true) then\012\009\009\009SP('Test','visible',true);\012\009\009else\012\009\009\009SP('Test','visible',false);\012\009\009end if;\012   \009\009\012\009end proc:\012\012\009########################################\012\009# Setting up the problem\012\009########################################\012\009SetProblem := proc()\012\009\009local a, b,c, d,helix, orient, Fig, Rot;\012\009\009randomize();\012\009\009\012\009\009a := RG(integer(range = -3..3));\012\009\009b := RG(integer(range = -2..2)); \012\012\009\009c := RG(integer(range = 1...10))*2*Pi/10; \012\009\009d := RG(integer(range = 1...10))*2*Pi/10;\012 \012\009\009Rot:= Matrix(3,3, [[1,0,0],[0, cos(d), -sin(d)], [0, sin(d), cos(d)]]). Matrix(3,3, [[cos(c), -sin(c), 0],[sin(c), cos(c), 0], [0,0,1]]);\012\009\009\012\009\009# We need to make sure that at most one of the two variables are zero.\012\009\009if evalb(a = 0) or evalb(b = 0) then\012\009\009\009Actions:-SetProblem();\012\009\009\009return;\012\009\009end if;\012\012\009\009if evalb(b &lt; 0) then\012\009\009\009orient := [145, -80, 0];\012\009\009\009SOL[1] := 'V';\012\009\009else\012\009\009\009orient := [145, 80, 0];\012\009\009\009SOL[1] := 'H';\012\009\009end if;\012\012\009\009SP('Test','value',[SOL[1], 'Regul\303\246r']);\012\009\009\012     \009helix :=unapply(convert(evalf(Rot.Vector([a*cos(t), a*sin(t), b*t])), list), t);\012\012\012\009\009Fig:= T-&gt; tubeplot(helix(t), t=0...T, radius=0.2, color = cyan, scaling = constrained, tickmarks = [0,0,0], orientation = orient);\012\012          SP('Plot', 'value', plots:-display(Fig(2*Pi)));\012\012          #SP('Plot', 'value', plots:-display(seq(Fig(q*2*Pi/10), q=0...10), insequence=true));\012          \012\009\009#SP('Plot','value', plots:-animate(tubeplot, [helix(x), x = 0 .. t,radius = 0.2], t = 0 .. 2*Pi, color = cyan, scaling = constrained, tickmarks = [0,0,0], orientation = orient));\012\009\009\012\009\009\012\009end proc:\012\012\009########################################\012\009# Input check\012\009########################################\012\009GetInput := proc()\012\009\009local UserSol, grad, i;\012\009\009global Grad;\012\009\009grad := [0.0, 0.0]: \012\012\009\009for i from 1 to 6 do\012\009\009\009UserSol[i] := parse(GP(cat('RadioButton',i),'value'));\012\009\009end do;\012\012\009\009if ( SOL[1] = 'V') then\012\009\009\009if ( UserSol[1] = true) then\012\009\009\009\009grad[1] := 1.0;\012\009\009\009else\012\009\009\009\009grad[1] := 0.0;\012\009\009\009end if;\012\009\009else\012\009\009\009if ( UserSol[2] = true) then\012\009\009\009\009grad[1] := 1.0;\012\009\009\009else\012\009\009\009\009grad[1] := 0.0;\012\009\009\009end if;\012\009\009end if;\012\012\009\009\012\009\009\012\009\009if (UserSol[6] = true) then\012\009\009\009grad[2] := 1.0;\012\009\009else\012\009\009\009grad[2] := 0.0;\012\009\009end if;\012\012\009\009Grad := evalf[3]((grad[1]+grad[2])/2);\012\012\009\009SP('Grade','value', Grad);\012\009\009\012\009end proc:\012\012\009#######################################\012\009# Reset\012\009#######################################\012\009Reset:= proc()\012\009\009Grad := 0.0;\012\009\009\012\009\009SP('Grade','value', NULL);\012\009\009SP(cat('RadioButton', 3),'value', true);\012\009\009SP(cat('RadioButton', 4),'value', true);\012\009\009\012\009\009\012\009\009Actions:-SetProblem();\012\009\009\012\009end proc:\012\009\012\009#######################################\012\009# Grade\012\009#######################################\009\012\009Grade := proc()\012\009\009\012\009\009return Grad\012\012\009end proc:\012\009\012end module:\012\012Actions:-Initialize();"/>
