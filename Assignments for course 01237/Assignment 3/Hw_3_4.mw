<Startup-Code startupcode="###########################################################\012# M\303\266bius Hjemmeopgaves\303\246t 3 til 01237 Differentialgeometri \012#  og parametrisk design\012#  Opgave 5: Klothoide\012#     - Line Glade, 19/09/22\012###########################################################\012restart;\012with(combinat): with(ListTools):with(LinearAlgebra):\012with(RandomTools): with(plots): with(plottools): with(ArrayTools):\012\012macro(\012SP = DocumentTools:-SetProperty, \012GP = DocumentTools:-GetProperty,\012RG = RandomTools:-Generate\012):\012\012Actions := module()\012\009export Initialize, GetInput, SetProblem, Grade, Reset, Func;\012\009local  SOL,  grad, solX, solY, x, y;\012\009global Grad, TEST;\012\012\009# Test variable: Set to false before export\012\009TEST := false;\012\012\009########################################\012\009# Initialization\012\009########################################\012\009Initialize:= proc()\012\009#\009local i;\012\009\012\009\009Grad := 0;\012\009\009\012\009\009SP('Grade','value',NULL);\012\009\009SP('Report','value',NULL);\012\009\009SP('Interpreter','value',NULL);\012\012\009\009Actions:-SetProblem();\012\009\009\012\009\009## Test\012\009\009if evalb(TEST = true) then\012\009\009\009SP('Test','visible',true);\012\009\009else\012\009\009\009SP('Test','visible',false);\012\009\009end if;\012   \009\009\012\009end proc:\012\012\009########################################\012\009# Setting up the problem\012\009########################################\012\009SetProblem := proc()\012\009\009local x0, y0, solX, solY, p1, p2;\012\009\009global s, t;\012\012\009\009randomize();\012\009\009x0 := RG(integer(range = -2..2));\012\009\009y0 := RG(integer(range = -2..2));\012\012\009\009SP('Given_x','value',x0);\012\009\009SP('Given_y','value',y0);\012\012\009\009solX := dsolve({diff(x(s),s) = cos(s^2/2), x(0) = x0});\012\009\009solY := dsolve({diff(y(s),s) = sin(s^2/2), y(0) = y0});\012\012\009\009Func(solX);\012\009\009Func(solY);\012\012\009\009SOL := [limit(x(s),s=infinity), limit(y(s),s=infinity) ] ;\012\012\009\009p1 := plot({[x(t),y(t),t=-6..6]},color=red);\012\009\009p2 := plots:-pointplot([[SOL[1],SOL[2]]],color = blue, symbol = solidcircle, symbolsize = 11);\012\012\009\009SP('Plot','value',plots:-display(p1,p2,scaling=constrained, tickmarks = [0,0]));\012\009\009SP('Test','value', [SOL[1], SOL[2]]);\012\009\009\012\009end proc:\012\009\012\009########################################\012\009# Helper-function for defining the parameterization of the clothoid\012\009########################################\012\009Func := proc(g)\012    \009\009assign(op(0, lhs(g)) = unapply(rhs(g), op(lhs(g))));\012    \009\009op(0, lhs(g));\012\009end proc:\012\012\009########################################\012\009# Input check\012\009########################################\012\009GetInput := proc()\012\009\009local UserSol, uRaw, soldiff, solcrit, catch_input, i, unpack, num, numsol, listnum, listnumsol, userflat, solflat;\012\009\009global Grad;\012\009\009\012\009\009grad := 0.0;\012\009\009catch_input := 0;\012\009\009uRaw := GP('Input','value');\012\012\009\009if evalb(uRaw = &quot;&quot;) then\012\009\009\009SP('Report','value',&quot;Missing input&quot;);\012\009\009else\012\009\009\009try\012\009\009\009\009parse(uRaw);\012\009\009\009catch :\012\009\009\009\009catch_input := 1;\012\009\009\009end try;\012\012\009\009\009if evalb( catch_input = 0) then\012\012\009\009\009\009unpack := [parse(uRaw)];\012\009\009\009\009num := nops(Flatten(unpack,1));\012\009\009\009\009numsol := nops(Flatten([SOL],1));\012\009\009\009\009listnum := nops(Flatten(unpack));\012\009\009\009\009listnumsol := nops(Flatten([SOL]));\012\012\009\009\009\009if not( num = numsol and listnum = listnumsol and whattype(evalf(add(Flatten(unpack)[i],i=1...listnum))) = float) then\012\009\009\009\009\009SP('Report','value',&quot;Wrong dimensions of the input or wrong type of elements&quot;);\012\009\009\009\009\009SP('Interpreter','value',Flatten(unpack));\012\009\009\009\009else\012\009\009\009\009\009SP('Report','value', NULL);\012\009\009\009\009\009SP('Interpreter','value',Matrix(Flatten(unpack,1)));\012\009\009\009\009\009\012\009\009\009\009\009userflat:= evalf(Flatten(unpack));\012\009\009\009\009\009solflat:= evalf(Flatten(SOL));\012\009\009\009\009\009soldiff := abs(evalf[3](Norm(Vector(userflat-solflat),2)));\012\009\009\009\009\009solcrit := evalf[3](0.1*Norm(Vector(solflat), 2));\012\009\009\009\009\009\012\009\009\009\009\009if evalb(soldiff = 0.0) then\012\009\009\009\009\009\009grad := 1.0;\012\009\009\009\009\009elif evalb(soldiff &lt;= solcrit) then\012\009\009\009\009\009\009grad := 0.5;\012\009\009\009\009\009end if;\012\009\009\009\009end if;\012\009\009\009else\012\009\009\009\009SP('Report','value',&quot;Syntaksfejl&quot;);\012\009\009\009\009SP('Interpreter','value',Error);\012\009\009\009end if;\012\009\009end if;\012\012\009\009SP('Grade','value', grad);\012\009\009Grad := grad;\012\009\009\012\009end proc:\012\012\009########################################\012\009# GRADE\012\009########################################\012\009Grade := proc();\012\012\009\009return Grad;\012\009\009\012\009end proc:\012\012\009#######################################\012\009# RESET\012\009#######################################\012\009Reset:= proc()\012\012\009\009SP('Grade','value',NULL);\012\009\009SP('Report','value',NULL);\012\009\009SP('Interpreter','value',NULL);\012\009\009SP('Input','value',NULL);\012\012\009\009# Unassigning the variables for the function\012\009\009unassign(solX, solY, x, y);\012\009\009\012\009\009Actions:-SetProblem();\012\009end proc:\012\009\012end module:\012###########################################################\012\012Actions:-Initialize();\012"/>
